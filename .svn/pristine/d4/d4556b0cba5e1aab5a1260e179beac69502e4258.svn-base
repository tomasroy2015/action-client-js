

/*-----------------------------------
scorebar directive style
-------------------------------------*/
.lisview-container {

    padding-top: 50px;
    width: 100%;
}

.scorebar-mainDiv{
    padding: 0;margin: 0;width: 100%;float: left
}

.scorebar-scoreDiv{
    margin-top: 0px;width: 30px;display: inline-block;float: left
}

.scorebar-scorebarCanvas{
    padding-left: 0;padding-right: 0;margin-left: auto;margin-right: auto;display: block;float: left
}

/*Question score box style*/
.horizontal { display: inline; float: left; }

