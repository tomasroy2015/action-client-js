.question{
    display: table;
    width: 100%;
    padding: 0px;
    margin: 0px;
    position: relative;
}
.questionListItem{
    display: block;
    height: 30px;
    position: relative;
    padding: 0px;
    margin: 0px;
    text-align: center;
    border: 1px solid transparent;
}
.questionListItem:hover{
    background-color: #c9c9c9;
    cursor: pointer;
}
.selectedQuestionItem>div:before {
    border-color: transparent transparent transparent #c9c9c9;
}
.selectedQuestionItem{
    background-color: #c9c9c9;
    cursor: pointer;
}
.questionListItemDiv{
    position:relative;
    line-height: 30px; /* equal to the list item's height */
    height:100%;
    width: 100%;
}
.questionListItemDiv:after,.questionListItemDiv:before{
    content:"";
    display:inline-block;
    border-width: 16px;
    border-style: solid;
    width: 0px;
    height: 0px;
    left: 100%;
    top: -1px;
    position: absolute;
    z-index: 1;
}
.questionListItemDiv:after{
    border-color: transparent transparent transparent #ffffff;
}
.questionListItem:hover .questionListItemDiv:before{
    border-color:transparent transparent transparent #c9c9c9;
}
.questionListItemDiv:before{
    border-width: 14.5px;
    display: block;
    border-color: transparent transparent transparent #ffffff;
    z-index: 2;
    top:0px;
}
#selectable .ui-selected .questionListItemDiv:before {
    border-color: transparent transparent transparent #c9c9c9;
}
#selectable .ui-selected {
    background-color: #c9c9c9;
    cursor: pointer;
}
