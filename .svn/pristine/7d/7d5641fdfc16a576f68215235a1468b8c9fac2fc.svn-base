<link rel="stylesheet" href="Views/ServiceArea/ServiceArea.tpl.css"/>
<div id="serviceArea">
    <div class="btn-group" data-toggle="buttons">
        <label class="btn btn-primary {{ $index == 0 ? 'active' : ''}}" ng-repeat="eachSA in serviceAreaCollection track by $index">
            <input type="radio" name="options" id="{{eachSA.Id}}" autocomplete="off"
                   value="{{eachSA.DisplayText}}"> {{eachSA.DisplayText}}
        </label>
    </div>
</div>
