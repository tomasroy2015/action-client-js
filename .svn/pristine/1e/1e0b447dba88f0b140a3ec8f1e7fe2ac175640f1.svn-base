/* Accordion panel heading section
======================================================*/
[id^='accordion-panel'].panel-default.panel {
  margin-bottom: -5px;
  background-color: #FFFFFF;
  border-radius: 0;
  -webkit-box-shadow: none;
  box-shadow: none;
  border: none; }
  [id^='accordion-panel'].panel-default.panel:last-child {
    margin-bottom: 0;
    background-color: #FFFFFF;
    border-radius: 0;
    -webkit-box-shadow: none;
    box-shadow: none;
    border-bottom: 1px solid #000000; }
[id^='accordion-panel'].panel-default .panel-collapse .panel-body {
  padding: 0 !important;
  padding-left: 15px !important;
  padding-right: 15px !important;
  border-top: none; }
[id^='accordion-panel'].panel-default .panel-heading {
  padding: 3px 15px;
  cursor: pointer;
  background: transparent;
  border-top: 1px solid #000000;
  border-bottom: none;
  border-radius: 0; }
  [id^='accordion-panel'].panel-default .panel-heading .panel-title {
    margin-left: -12px;
    margin-top: 2px;
    padding: 2px 0; }
    [id^='accordion-panel'].panel-default .panel-heading .panel-title > a:focus, [id^='accordion-panel'].panel-default .panel-heading .panel-title > a:active {
      -moz-outline-style: none;
