  font-size: 12px;
  font-weight: bold; }

#glypContainer {
  font-size: 16px;
  right: -15px; }

/* Data filter items section
======================================================*/
a.list-group-item {
  width: 222px;
  height: 20px;
  padding: 3px 8px;
  color: #000000;
  background: #C6C6C6;
  margin: 2px 0;
  font-size: 0.9em;
  font-weight: bold; }
  a.list-group-item:first-child {
    border: none;
    border-top-right-radius: 0;
    border-top-left-radius: 0; }
  a.list-group-item:last-child {
    margin-bottom: 8px;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0; }
  a.list-group-item:hover {
    color: #000000;
    text-decoration: none;
    background-color: #ddddde; }
    a.list-group-item:hover:after {
      top: 0;
      content: "";
      position: absolute;
      right: -10px;
      border-top: 10px solid transparent;
      border-bottom: 10px solid transparent;
      border-left: 10px solid #ddddde;
      z-index: 100; }
  a.list-group-item:focus {
    background: #C6C6C6; }
    a.list-group-item:focus:after {
      top: 0;
      content: "";
      position: absolute;
      right: -10px;
      border-top: 10px solid transparent;
      border-bottom: 10px solid transparent;
      border-left: 10px solid #C6C6C6;
      z-index: 100; }
  a.list-group-item:after {
    top: 0;
    content: "";
    position: absolute;
    right: -10px;
    border-top: 10px solid transparent;
    border-bottom: 10px solid transparent;
    border-left: 10px solid #C6C6C6;
    z-index: 2000; }
  a.list-group-item.df-selected {
    background: #FFED00; }
    a.list-group-item.df-selected:after {
      top: 0;
      content: "";
      position: absolute;
      right: -10px;
      border-top: 10px solid transparent;
      border-bottom: 10px solid transparent;
      border-left: 10px solid #FFED00;
      z-index: 100; }

/* Reset filter button section
======================================================*/
.reset-filter {
  padding: 15px 6px; }
  .reset-filter > .btn-default {
    text-shadow: none;
    border-color: transparent;
    box-shadow: none;
    border-radius: 0; }

.btn-reset {
  text-decoration: none;
  float: right;
  color: #000000;
  background: transparent; }
  .btn-reset img {
    padding-left: 5px; }
  .btn-reset:focus {
    -moz-outline-style: none;
    -webkit-outline-style: none;
    outline: none;
    background: transparent; }
  .btn-reset:active, .btn-reset:hover {
    background: #ddddde; }

