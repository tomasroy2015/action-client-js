/*}*/

/*.actionScoreBar-scorebarCanvas{*/
/*padding-left: 0;padding-right: 0;margin-left: auto;margin-right: auto;display: block;float: left*/
/*}*/
.listview-container .vscrollbar {
    overflow-x: hidden; /*for hiding horizontal scroll bar*/
    overflow-y: auto; /*for vertical scroll bar*/
}

/*list pagination */
.listview-container .pagination {
    display: inline-block;
    border-radius: 1px;
    padding: 0;
    margin-bottom: 3px;
    margin-top: -10px;
}

/*Question score box style*/
.listview-container .horizontal {
    display: inline;
    float: left;
}
/*list view minimum evaluation number input type style*/
.listview-container .btn-list-view-min-evaluation-apply {
    height: 22px;
    border: none;
    border-radius: 0;
    background: #878787 !important;
    margin-left: -3px;
    padding-top: 2px;
    color: white;
    width: 58px;
}

.listview-container .btn-list-view-min-evaluation-apply:hover {
    background: #555759 !important;
}
.listview-container .min-evaluation-container-div{
    float: right;display: inline-block;
}

.listview-container .list-view-apply-reset-container-div {

    padding-top: 20px;
    height: 62px;
    display: inline-block;
    padding-left: 10px;
}
@media (min-width: 1208px) {
    .listview-container .col-sm-6 {
        width: 40%;
    }

    .listview-container .col-sm-3 {
        width: 20%;
        padding-left: 0;
        padding-right: 0;
    }

    .listview-container .col-sm-7 {
        width: 40%;
        padding-left: 0;
        padding-right: 0;
    }
}

@media (max-width: 1207px) {
    .listview-container .col-sm-6 {
        width: 40%;
    }

    .listview-container .col-sm-3 {
        width: 25%;
        padding-left: 0;
        padding-right: 0;
    }

    .listview-container .col-sm-7 {
        width: 61%;
        padding-left: 22px;
        padding-right: 0;
    }
    .listview-container .min-evaluation-container-div {
        float: left;
        display: inline-block;
    }

}

