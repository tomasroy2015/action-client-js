/*.ui-grid-viewport{*/
    /*height: 100% !important;*/
/*}*/
.listview-container .table-responsive{
    height: 100% !important;
    /*border: 1px solid blue;*/
}
.listview-container .ui-grid-container {
    position: relative;
    height: 90%;
}
.listview-container .ui-grid-container >.ui-grid {
    position : absolute;
    width : 100% !important;
    height : 100% !important;
    border: 1px solid #008000;

    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
}
