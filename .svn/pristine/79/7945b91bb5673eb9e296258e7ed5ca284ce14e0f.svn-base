.listview-container .reset-filter-evaluation {
    display: inline-block !important;
    margin-left: 5px !important;
    width: 70px !important;
    cursor: pointer;
}

.listview-container .reset-filter-button {
    height: 22px;
    width: 71px;
    border: none;
    border-radius: 0;
    background: transparent !important;
}

.listview-container .reset-filter-button:hover {
    background: #c8c8c8 !important;
}

.listview-container input[type="number"]::-webkit-outer-spin-button,
.listview-container input[type="number"]::-webkit-inner-spin-button {
    -webkit-appearance: none !important;
    margin: 0 !important;
}

.listview-container input[type="number"] {
    -moz-appearance: textfield !important;
}

.listview-container button[disabled], .listview-container html input[disabled] {
    background: transparent !important;
    opacity: 0.6 !important;
    -ms-filter: alpha(opcity=40) !important;
    filter: alpha(opcity=40) !important;

}

.listview-container button[disabled]:hover {
    background: transparent !important;
}
.show-all-grp-expand-container-div input[type="checkbox"],
.show-all-grp-expand-container-div .checkbox-inline input[type="checkbox"],
.show-all-grp-expand-container-div .threshold-checkbox {
    font-weight: bold;
    margin-top: 0px;
}

