/*-----------------------------------
actionScoreBar directive style
-------------------------------------*/
.listview-container {
    padding-top: 0px;
    /*width: 98%;*/
    margin-top: 0px;
    /*border : 1px solid deeppink;*/
    height: 100%;
    min-height: 100%;
    position: relative;
}

.listview-container .view-by-attribute-container-div{
    /*width: 35%;*/
    display: inline-block;
}
.listview-container .view-by-attribute-container-div-parent{
    width: 100%;text-align: left
}
.listview-container .show-all-grp-expand-container-div{
    float: left;display: inline-block
}

.listview-container .show-all-grp-expand-container-div-parent{
    height:62px;padding-top: 13px ;display: inline-block;
    /*width: 25%;*/
}
.listview-container .btn-apply-container-div{
    display: inline-block; margin-left: 5px
}
.listview-container .min-evaluation-number-elm{
    width: 51px;height: 22px; border-radius: 0;
}
.listview-container .disable-label{
    opacity: 0.5;
}
.listview-container .question-score-directive-font{
    font:bold 12px AkkuratStd-Regular
}
.listview-container .question-score-directive-div{
    padding: 0;margin: 0;width: 100%;float: left
}
/*.question-score-directive-div .tooltip{*/
   /*width: 100%;*/
   /*padding: 2px;*/
/*}*/

.listview-container .question-score-directive-div .tooltip-inner {
    max-width: 100%;
    padding: 3px 8px;
    color: #ffffff;
    text-align: center;
    text-decoration: none;
    background-color: #000000;
    border-radius: 4px;
    width: 100%;
    overflow: visible;
}
.listview-container .question-score-directive-svg{
    float: left;padding: 0;margin: 2px;display: inline
}

.listview-container .row-container {
    margin-right: 0;
    margin-left: 0;
    /*border: 2px solid firebrick;*/
    height: 100%;
}

.listview-container .selected {
    background-color: #c8c8c8;
}

.listview-container td div {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.listview-container .table tbody > tr div {
    height: 40px;
}

.listview-container .table thead > tr > th {
    vertical-align: bottom;
    border-bottom: 1px solid #000000;
}

#nameContainerDiv.row {
    margin-right: 0px !important;
    margin-left: 0px !important;
}

#childItemNameContainerDiv.row {
    margin-right: 0px !important;
    margin-left: 0px !important;
}

#tbl > tbody > tr > td > div {
    padding-top: 5px;

}

.listview-container .nameColumnPadding {
    padding-top: 7px !important;
}

.listview-container .scoreBarColumnPadding {
    padding-top: 8px !important;
}

.listview-container .name {
    line-height: 27px;
}

.listview-container .toggleRow {
    display: none;
}

/*
#tbl>tbody>tr:hover { background: #c8c8c8 !important; }*/

#tbl > tbody > tr:hover {
    background: #ddddde !important;
}

#tbl > tbody > tr:hover .speech {
    background: #ffffff !important;
}

#tbl > tbody > tr:hover .speech:before {
    border-color: #ffffff transparent transparent #ffffff;
}

/*header column arrow icon style*/
#tbl.ng-table #tbl.th.sortable #tbl.div:after, #tbl.ng-table #tbl.th.sortable #tbl.div:before {
    content: "";
    border-width: 0 4px 4px;
    border-style: solid;
    border-color: #000 transparent;
    visibility: visible;
    right: 110px;
    top: 30%;
    position: absolute;
    opacity: .3;
    margin-top: -4px;
}

#tbl > tbody > tr {
    padding-bottom: 0px !important;
    cursor: pointer;
}

#tbl > tbody > tr > td {
    padding: 0px !important;
}

#tbl.table > tbody + tbody {
    border-top: 0px solid #dddddd;
}

/*list template main div */
#parentDiv.content-main {
    min-height: 100%;
    height: 100%;
    /* margin: 0 auto; */
    margin-left: 15px;
    margin-right: 15px;
    /*padding-left: 10px;*/
    /*padding-right: 15px;*/
    /*border: 1px solid #FF0000;*/
}

