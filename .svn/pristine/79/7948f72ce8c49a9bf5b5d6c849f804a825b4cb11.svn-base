.container{
    background-color: #ffffff;
}
.container.more-settings-container{
    color: #000000;
    width: 230px;
    height: 455px;
    font-family: "AkkuratStd-Regular", "Segoe UI Light", Frutiger, "Frutiger Linotype", "Dejavu Sans", "Helvetica Neue", Arial, sans-serif;
    border: 1px solid #c0c0c0;
    border-radius: 2px;
    top: 250px;
    box-shadow: 2px 2px 2px #d3d3d3;
    cursor: default;
}
.page-header {
    margin: 10px 0 0px;
    border-bottom: 1px solid #000000;
}
.page-header.moresettings-page-header{
    font-size: 14px;
    height: 30px;
    font-weight: bold;
}
.more-settings-body{
    font-size: 11px;
}
.radio.moresettings-radio{
    font-weight: normal;
}
.common-moresettings-style{
    margin: 2px 0 0px;
    border: none;
    border-bottom: 1px solid #000000;
}
.more-settings-body > .more-settings-panel-container{
    margin: 2px 0 0px;
    border: none;
    border-bottom: 1px solid;
    height: 100px;
}
.more-settings-body > .custom-value-container{
    margin: 2px 0 0px;
    border: none;
    border-bottom: 1px solid  #d3d3d3;
}
.more-settings-body > .more-settings-panel-container > .moresettings-value-container{
    background-color: #d3d3d3;
    border: none;
    height: 60px;
}

.more-settings-body > .color-settings-panel-container{
    margin: 2px 0 0px;
    border: none;
    border-bottom: 1px solid #000000;
    height: 160px;
}
.more-settings-body > .color-settings-panel-container > .color-set-container{
    background-color: #d3d3d3;
    border: none;
    height: 120px;
}

.more-settings-body > .more-settings-panel-container > .moresettings-value-container > .moresettings-radio-group {
    padding-top: 2px;
    padding-left: 10px;
}
.more-settings-body > .color-settings-panel-container > .color-set-container > .moresettings-radio-group {
    padding-top: 2px;
    padding-left: 10px;
}
.moresettings-button-container{
    position: relative;
    width: 100%;
    height: 40px;
    padding-top: 10px;
    color: #ffffff;
}

.moresettings-button-position {
    position: absolute;
    text-align: center;
    left: 42%;
}

.moresettings-btn-white-bg {
    padding-top: 0;
    height: 26px;
    line-height: 26px;
    background-color: #555759;
    color: #fff;
    border-radius: 0;
}
.btn-moresettings-ok {
    position: absolute;
    text-align: center;
    left: 42%;
    height: 26px;
    border        : none;
    border-radius : 0;
    color: #fff;
    background    : #878787 !important;
}

.btn-moresettings-ok:hover {
    background : #555759 !important;
}
input[type="checkbox"], .checkbox-inline input[type="checkbox"] {
    float: left;
    margin-left: -20px;
    margin-top: 0px;
}
input[type="checkbox"], .checkbox-inline input[type="checkbox"],.moresettings-checkbox{
    font-weight: normal;
}

.row .respondent-label {
    margin-left: 25px;
    padding-top: 8px;
    height: 20px;
    font-weight: normal;
}
.more-settings-common-label{
     font-weight: bold;
}
#txtPerSite{
      width: 36px;
      height: 18px;
      margin-left: 25px;
}
#txtPerCountry{
    width: 36px;
    height: 18px;
    margin-left: 4px;
}