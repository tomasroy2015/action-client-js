[id^=serviceArea-] {
  padding-top: 2px;
}
[id^=serviceArea-] .btn-group > .btn:first-child {
  margin-left: 0;
}
[id^=serviceArea-] .btn {
  width: 30px;
  height: 18px;
  line-height: 18px;
  padding: 0;
  margin-bottom: 0;
  margin-right: 10px;
  font-size: 1em;
  font-weight: bold;
  color: #000000;
  border: 1px solid transparent;
  border-radius: 0;
  box-shadow: none;
}
[id^=serviceArea-] .btn-primary {
  background-color: #C6C6C6;
  box-shadow: none;
}
[id^=serviceArea-] .btn-primary:hover, [id^=serviceArea-] .btn-primary:focus, [id^=serviceArea-] .btn-primary:active,
[id^=serviceArea-] .btn-primary .open .dropdown-toggle.btn-primary {
  background-color: #ddddde;
}
[id^=serviceArea-] .btn-primary.active {
  padding-top: 1px;
  background-color: #FFED00;
  border: none;
}

[id^=serviceArea-] .btn:not(:last-child):after {
  content: "";
  display: inline-block;
  position: absolute;
  color: #000;
  right: -10px;
  width: 10px;
  border-bottom: 1px solid #000;
  top: 50%;
}

