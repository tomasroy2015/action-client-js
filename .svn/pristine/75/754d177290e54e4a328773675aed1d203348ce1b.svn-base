/* Page styles
======================================================*/
.scorecard-main-wrapper {
  background-color: #FFFFFF;
  width: 298px;
  font-size: 1em;
  position: relative;
  top: 0; }
  .scorecard-main-wrapper > .ac-list-group {
    padding-left: 0;
    margin-bottom: 20px; }
    .scorecard-main-wrapper > .ac-list-group .ac-list-group-item {
      position: relative;
      display: block;
      padding: 2px 4px;
      margin-bottom: -1px; }
      .scorecard-main-wrapper > .ac-list-group .ac-list-group-item:last-child {
        margin-bottom: 0; }

.ac-list-group-item .question-wrapper {
  height: 25px;
  line-height: 25px; }
  .ac-list-group-item .question-wrapper .question-text {
    display: inline-block;
    width: 152px;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    vertical-align: middle; }
.ac-list-group-item .ac-list-group-item-scorebar {
  display: inline-block;
  vertical-align: middle; }

/* Header section
======================================================*/
.header-container {
  width: 100%;
  height: 32px;
  line-height: 32px;
  background: #ddddde;
  position: relative;
  margin-bottom: 2px; }
  .header-container .main-title-container {
    height: 18px;
    line-height: 18px; }
  .header-container .sub-title-container {
    height: 14px;
    line-height: 14px;
    font-size: .9em;
    margin-top: -1px; }
  .header-container .main-title, .header-container .sub-title {
    width: 185px;
    max-width: 185px;
    padding-left: 5px;
    font-weight: bold;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    display: block; }

