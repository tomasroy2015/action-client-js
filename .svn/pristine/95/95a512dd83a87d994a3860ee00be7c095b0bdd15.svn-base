/* Variables
======================================================*/
.data-filter-container {
  background-color: #878787;
  width: 100%;
  height: 100%;
  position: relative;
}
.data-filter-container .left-arrow-container {
  position: absolute;
  background: #878787;
  right: -16px;
  top: 10px;
  text-align: center;
  -webkit-box-shadow: 0 0 4px 2px rgba(85, 87, 89, 0.5);
  -moz-box-shadow: 0 0 4px 2px rgba(85, 87, 89, 0.5);
  box-shadow: 0 0 4px 2px rgba(85, 87, 89, 0.5);
  height: 20px;
  width: 16px;
}
.data-filter-container .left-arrow-container .fa-caret-left {
  color: #fff;
  font-size: 1.2em;
  display: table-cell;
  vertical-align: middle;
  height: 20px;
  width: 16px;
}
.data-filter-container .left-arrow-container:hover {
  cursor: pointer;
}
.data-filter-container .left-arrow-container:hover .fa-caret-left {
  color: rgba(255, 255, 255, 0.7);
}
.data-filter-container .datfilter-header-wrapper {
  background-color: #878787;
  position: relative;
  height: 40px;
  top: 0;
}
.data-filter-container .datfilter-header-wrapper > .datafilter-title {
  font-size: 1.2em;
  /*   font-weight: bold; */
  color: #ffffff;
  margin-left: 18px;
  margin-top: 12px;
}

.datfilter-footer-wrapper {
  background-color: #878787;
  position: relative;
  width: 100%;
  height: 45px;
  bottom: 0;
}
.datfilter-footer-wrapper > .btn-wrapper {
  background-color: #ddddde;
  /*   margin-left: auto;
    margin-right: auto;
    width: 60px;
    height: 45px; */
  width: 60px;
  height: 24px;
  /*   overflow: hidden; */
  margin: auto;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
}
.datfilter-footer-wrapper > .btn-wrapper > .btn-apply {
  background: #C6C6C6;
  text-shadow: none;
  border-color: transparent;
  box-shadow: none;
  border-radius: 0;
  width: 100%;
  height: 100%;
  line-height: 100%;
}

.grid-wrapper {
  height: 535px;
  width: 385px;
  max-width: 385px;
  position: relative;
  margin-left: 15px;
  margin-right: 15px;
}
.grid-wrapper > .grid {
  position: absolute;
  width: 100% !important;
  height: 100% !important;
  color: #fff;
}
.grid-wrapper .ui-grid {
  border: none !important;
}
.grid-wrapper .ui-grid-cell-contents {
  background-color: #878787 !important;
}
.grid-wrapper .ui-grid-header-viewport {
  background-color: #878787 !important;
}
.grid-wrapper .ui-grid-cell:not(.ui-grid-header-cell) {
  border-bottom: 1px solid #fff !important;
  border-right: none !important;
  box-sizing: border-box;
}
.grid-wrapper .ui-grid-header {
  border-bottom: 1px solid #fff !important;
  border-right: none !important;
  /*   width:96% !important; */
  margin-right: 17px !important;
}
.grid-wrapper .ui-grid-header-cell {
  border-right: none !important;
}
.grid-wrapper .ui-grid-viewport {
  overflow-x: hidden !important;
  overflow-y: auto !important;
}
.grid-wrapper .ui-grid-row {
  border-bottom: 1px solid #fff !important;
  box-sizing: border-box;
}
.grid-wrapper .ui-grid-pinned-container .ui-grid-viewport {
  overflow-x: hidden !important;
  overflow-y: hidden !important;
}
.grid-wrapper .ui-grid-icon-ok:before {
  content: '' !important;
  width: 10px;
  height: 10px;
  background-color: #ffffff;
}

/* Scrollbar
======================================================*/
.ui-grid-viewport::-webkit-scrollbar-thumb {
  background-color: #555759;
  border: 1px solid transparent;
  border-radius: 5px;
  background-clip: content-box;
}

.ui-grid-viewport::-webkit-scrollbar-thumb:hover {
  background-color: #555759;
}

.ui-grid-viewport::-webkit-scrollbar-thumb:active {
  background-color: #555759;
}

.ui-grid-viewport::-webkit-scrollbar-track {
  border-radius: 5px;
  /*   -webkit-box-shadow: 0 0 6px gray inset; */
}

.ui-grid-viewport::-webkit-scrollbar {
  width: 10px;
  background-color: #ddddde;
  border-radius: 5px;
}

