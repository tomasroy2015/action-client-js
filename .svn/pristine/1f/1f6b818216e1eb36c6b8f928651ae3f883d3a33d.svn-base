html, body {
    min-height : 100%;
    height : 100%;
    /*margin: 0;*/

}

.container-fluid,.row-fluid {
    min-height : 100%;
    height     : 100%;
    padding : 0;
}


.col-md-3.left-menu, .col-md-9.right-content {
    min-height : 100%;
    height     : 100%;
}

.col-md-3.left-menu {
    /*background-color : #204d74;*/
    border           : 1px solid #ff0000;
    padding          : 10px;
    color            : #efefef;
}

.col-md-9.right-content {
    /*background-color : #3e8f3e;*/
    border           : 1px solid #ff0000;
    /*padding          : 10px;*/
    padding          : 0;
}


/* css custom */


.content-page-header {
    height: 80px;
    background-color : #878787;
    margin: 0;
    padding: 0;
}

.content-main {
    margin: 0;
    padding: 10px;
}


