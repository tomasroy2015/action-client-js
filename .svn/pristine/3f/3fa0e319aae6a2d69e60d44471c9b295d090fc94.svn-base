.ng-modal-container .ng-modal-overlay {
    /* A dark translucent div that covers the whole screen */
    position:absolute;
    z-index:9999;
    top:0;
    left:0;
    width:100%;
    height:100%;
    background-color:#ddddde;
    opacity: 0.8;
}
.ng-modal-container .ng-modal-dialog {
    /* A centered div above the overlay with a box shadow. */
    z-index:10000;
    position: absolute;
    width: 50%; /* Default */

    /* Center the dialog */
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    background-color: #fff;
    box-shadow: 4px 4px 80px #000;
}
.ng-modal-container .ng-modal-dialog-content {
    padding:0px;
    text-align: left;
}
.ng-modal-container .cursor{
    cursor: pointer;
}
.ng-modal-container .fa-lg{
    font-size: 19px;
}
.ng-modal-container .ng-modal-close {
    position: absolute;
    top: 3px;
    right: 5px;
    padding: 5px;
    cursor: pointer;
    font-size: 120%;
    display: inline-block;
    font-weight: bold;
    font-family: 'arial', 'sans-serif';
}
.ng-modal-container .confirm-btn-container-div{
    padding-top: 14px;
    padding-left: 30px;
    /* padding-right: 10px; */
    margin-right: 13px;
    text-align: center;
    padding-bottom: 30px;
}
.ng-modal-container .dialog-header{
    font-size: 16px;
    font-weight: bold;
    padding-bottom: 10px;
    min-height: 16.428571429px;
    background-color: #000000;
    color: white;
    padding-top: 15px;
    padding-right: 0px;
    padding-bottom: 15px;
    padding-left: 15px;
    /* border-bottom: 1px solid #e5e5e5; */
    height: 60px;
    display: inline-block;
    width: 100%;
}
.ng-modal-container .modal-title{
    font-size: 16px;
    font-weight: bold;
    width: 90%;
    display: inline-block;
}
.ng-modal-container .element-parent-div{
    padding: 43px;
    padding-bottom: 0px;
}
.ng-modal-dialog-content .element-container-div{
    height: 35px;
}
.ng-modal-dialog-content label{
    vertical-align: top;
}
.ng-modal-dialog-content label> input[type="checkbox"] {
    margin: 0;
}
.ng-modal-dialog-content .inline-block{
   display: inline-block;
}
.ng-modal-dialog-content .label-width{
    width: 105px;
}

.ng-modal-dialog-content .text-input-normal{
    width: 175px;
    height: 20px;
    border-radius: 0px;
}
.ng-modal-dialog-content .drop-down-normal{
    width: 175px;
}
.ng-modal-dialog-content .drop-down-small{
    width: 145px;
}
.ng-modal-dialog-content .form-control:focus{
    border-color:red ;
}
.ng-modal-container .btn{
    height: 22px;
    border: none;
    border-radius: 0;
    background: #878787 !important;
    margin-left: -3px;
    padding-top: 2px;
    color: white;
    width: 65px;
    margin-right: 10px;
}
.ng-modal-container .left-align{
    float: left;
}
.ng-modal-container .right-align{
    float: right;
}
.ng-modal-container .clear{
    clear: both;
}