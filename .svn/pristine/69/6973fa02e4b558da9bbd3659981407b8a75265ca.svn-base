.dataSet-logout-container .logout-url-container {
    right     : 60px;
    position  : absolute;
    transform : translateY(100%);
}

.dataSet-logout-right-arrow-container {
    right: -20px;
    top: -13px;
    position: absolute;
    transform: translateY(69%);
}

.dataSet-logout-arrow-container {
    right: -18px;
    top: -19px;
    position: absolute;
    transform: translateY(90%);
}

.dataSet-logout-arrow-container:hover
{
    text-decoration : none;
    color           : #555759;
}
.dataSet-logout-circle-arrow-size-2x {
    vertical-align: middle;
    padding-left: 5px;
    font-size: 1.25em;
}
