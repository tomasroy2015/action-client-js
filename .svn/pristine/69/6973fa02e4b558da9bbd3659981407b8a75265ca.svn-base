html {
  position: relative;
  min-height: 100%; }

body {
  /* Margin bottom by footer height */
  margin-bottom: 45px;
  min-height: 400px;
  /*400px*/
  color: #333333; }
.dataset-container {
  max-width: 680px;
  padding: 0 15px;
  min-width: 455px; }
  .dataset-container .page-header {
    border-bottom: none;}
    .dataset-container .page-header .panel {
      border-radius: 0; }
      .dataset-container .page-header .panel.panel-default {
        opacity: 0.9;
        /*height: 380px;*/
        /*350px*/
        width: 425px;
        margin-left: auto;
        margin-right: auto;
        border-radius: 0; }

.panel-heading {
  background-image: url("");
  background-color: #000 !important;
  color: #fff !important;
  height: 57px;
  line-height: 42px;
  font-size: 1.416666666666667em;
  /* 17px/12px */
  text-shadow: none;
  border-radius: 0; }
  .panel-heading .logo {
    float: right;
    margin-top: 10px;
    margin-right: 10px; }

.panel-body-container {
  padding: 18px; }
  .panel-body-container .content-heading {
    font-weight: bold;
    font-size: 1em;
    font-weight: bold;
    color: #000000}
  .panel-body-container #ddlClient {
    margin-left: 20px;
    padding-left: 3px;
    width: 180px;
    height: 24px;
    border-radius: 0;
    font-weight: normal; }

input[type='radio'] {
  margin-top: 0; }

#ddlMonthSelect input[type='radio'], #ddlQuarterSelection input[type='radio'] {
  margin-left: 5px;
  margin-top: 0;
  padding-left: 3px;
  width: 180px;
  height: 24px;
  border-radius: 0;
  font-weight: normal; }

.btn-group > div {
  margin-bottom: 5px; }
  .btn-group > div#monthDdlContainer, .btn-group > div#quarterDdlContainer {
    display: flex; }

select#ddlMonthSelect {
  width: 150px;
  height: 24px;
  margin-left: 20px; }

select#ddlQuarterSelection {
  width: 150px;
  height: 24px;
  margin-left: 14px; }

.form-group.form-checkbox-panel {
  margin-top: 20px;
  /*padding-left: 25px;*/
  /*padding-right: 25px;*/
}

.button-container {
    position: relative;
    color: #fff;
    margin-top: 10px;
}

.btn-white-bg {
  padding-top: 0;
  height: 26px;
  line-height: 26px;
  background-color: #555759;
  border-radius: 0; }
  .btn-white-bg:hover {
    background-color: #878787;
    color:#ffffff;
  }

.right-side {
    position: absolute;
    left: 37%;
}

#datePanel {
    border-bottom: 1px solid #d3d3d3;
    margin-left: 15px;
    width: 385px;
}

.form-horizontal{
    /*border-bottom: 1px solid #d3d3d3;*/
}

.form-horizontal .control-label {
    text-align: right;
    left: 20px;
    height: 18px;}

.dataset-sel-group{
    margin-left: 42px;
    margin-top: -16px;
}
.dataset-header-container{
    border-bottom: 1px solid #000000;
    font-weight: bold;
    font-size: 1em;
    font-weight: bold;
    color: #000000;
    margin-top: 18px;
}
.dataset-label{
    padding-left: -16px;
}
