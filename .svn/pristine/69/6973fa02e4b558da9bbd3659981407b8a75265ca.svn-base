.footer {
    position         : absolute;
    bottom           : 0;
    width            : 100%;
    /* Set the fixed height of the footer here */
    height           : 45px;
    background-color : #555759;
    min-width        : 455px;
    left             : 0;
}

.url-container {
    right     : 60px;
    position  : absolute;
    transform : translateY(100%);
}

.right-arrow-container {
    right     : 34px;
    position  : absolute;
    transform : translateY(69%);
}

.arrow-container {
    right     : 36px;
    position  : absolute;
    transform : translateY(90%);
}

.url-container > a {
    text-decoration : none;
    color           : #fff;
}

.url-container > a:hover {
    text-decoration : none;
    color           : #ddddde;
}


/*---------Settings-----*/
.dataSet-settings-logo {
    position: absolute;
    top: 13px;
    right: 10px;
    cursor: pointer;
}
.dataSet-settings-logo:hover
{
    color: #ddddde;
    opacity: .5;
    filter: alpha(opacity=50);
    filter: brightness(-60%);
    -webkit-filter: brightness(-60%);
}
/*-------------Log out button------------*/
.dataSet-logout-container {
    position: absolute;
    margin-top: 13px;
    height: 20px;
    right: 62px;
    cursor: pointer;
    font-size: 14px;
    color: #000 !important;
    font-family: "AkkuratStd-Regular";
}

.dataSet-header-font-size{
    font-size: 13px;
    /*font-family: "AkkuratStd-Regular", "Segoe UI Light", Frutiger, "Frutiger Linotype", "Dejavu Sans", "Helvetica Neue", Arial, sans-serif;*/
}
