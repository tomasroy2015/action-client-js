.modal {
  overflow-y: hidden;
  text-align: center;
}

.modal:before {
  display: inline-block;
  vertical-align: middle;
  content: " ";
  height: 100%;
}

.modal-dialog {
  display: inline-block;
  text-align: left;
  vertical-align: middle;
}

.evaluationSummaryContainer {
  height: 450px;
  width: 100%;
  background-color: #dddddd;
}

.evaluationSummaryHeader {
  width: 100%;
  height: 20px;
  background-color: #000000;
  color: white;
}
.evaluationSummaryHeader .close {
  margin: 1px 5px 1px 0px;
  opacity: 1;
  background-color: white;
  border-radius: 50%;
  width: 17px;
  height: 17px;
  font-size: 17px;
}

.btn.ac-btn-right-evaluation-summary {
  height: 18px;
  font-size: 0.9em;
  position: absolute;
  left: 350px;
  background: black;
  color: transparent;
  padding-top: 0;
  padding-bottom: 0;
  padding-left: 5px;
  border-radius: 0;
  box-shadow: none;
  width: 50px;
}

.btn.ac-btn-left-evaluation-summary {
  height: 18px;
  font-size: 0.9em;
  position: absolute;
  left: 175px;
  background: black;
  color: transparent;
  padding-top: 0;
  padding-bottom: 0;
  padding-left: 5px;
  border-radius: 0;
  box-shadow: none;
  width: 50px;
}

.circle-arrow-right-size-2x {
  vertical-align: middle;
  padding-left: 10px;
  font-size: 1.5em;
  color: white;
}

.circle-arrow-left-size-2x {
  vertical-align: middle;
  padding-left: 10px;
  font-size: 1.5em;
  color: white;
}

.span-evaluation-count-container {
  position: absolute;
  left: 250px;
}

.headerContentContainer {
  align-self: center;
  width: 90%;
  margin: auto;
  color: white;
}

.questionListContainer {
  width: 45%;
  float: left;
}

.questionListHeader {
  margin: 5px 0px 0px 10px;
  font-size: 18px;
  height: 25px;
  width: 250px;
  max-width: 250px;
  overflow: hidden;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.questionListDetails {
  background-color: white;
  height: 385px;
  margin: 5px 0px 5px 10px;
}
.questionListDetails thead {
  flex-direction: column;
  align-items: stretch;
  display: inline-block;
  width: 260px;
}
.questionListDetails tbody {
  overflow-y: auto;
  height: 350px;
  display: inline-block;
  width: 260px;
}
.questionListDetails .table > thead > tr > th {
  vertical-align: middle;
  border: 0px;
}
.questionListDetails .table > tbody > tr > td, .questionListDetails .table > tfoot > tr > td {
  padding: 8px;
  line-height: 12px;
  vertical-align: middle;
  border: 0px;
}

.evaluationSummaryContent {
  width: 100%;
  height: 95%;
}

.othersInfoContainer {
  width: 52%;
  float: right;
}
.othersInfoContainer .divLabelContainer {
  height: 20px;
  margin: 5px 0px 0px 0px;
}

.otherInfoHeader {
  height: 25px;
  margin-top: 5px;
}

.regRemarksContainer {
  background-color: white;
  height: 80px;
  margin-top: 5px;
  font-size: 10px;
  overflow: auto;
}

.additionalRemarksWrapper {
  height: 100px;
}

.adiRemarksContainer {
  background-color: white;
  height: 80px;
  font-size: 10px;
  overflow: auto;
}

.attributeValueWrapper {
  height: 185px;
}

.attributeValueContainer {
  background-color: white;
  height: 175px;
  overflow: auto;
}

.headerLabel {
  vertical-align: bottom;
  float: left;
}

.translationDiv {
  float: right;
}
.translationDiv .btn {
  width: 30px;
  height: 18px;
  line-height: 18px;
  padding: 0;
  margin-bottom: 0;
  margin-right: 2px;
  font-size: 1em;
  font-weight: bold;
  color: black;
  border: 1px solid transparent;
  border-radius: 0;
  box-shadow: none;
  background-color: lightgrey;
}
.translationDiv .btn-primary {
  background-color: darkgray;
}

.attributeLabel {
  font-weight: bold;
}

.attributeValue {
  width: 150px;
}

.truncatedText {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.attributeList {
  display: table;
  width: 100%;
  padding: 0px;
  margin: 0px;
  position: relative;
}

.columnLeft {
  float: left;
}

.columnRight {
  float: right;
}

.attributeItemWrapper {
  margin-bottom: 5px;
}

.labelColumn {
  width: 200px;
}

.dataContainer {
  border-bottom: 1px solid black;
  height: 30px;
}

