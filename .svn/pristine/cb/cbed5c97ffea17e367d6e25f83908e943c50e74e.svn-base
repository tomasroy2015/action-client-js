<link rel="stylesheet" href="Views/DataFilter/GroupDeleteConfirmationWindowTemplate.css"/>
<div class="group-delete-confirmation-window-wrapper">
    <div class="modal-header">
        <span>{{MsgHeaderLbl}}</span>
    </div>
    <div class="modal-body">
        <div class="msg-body">
            {{MsgBody}}
        </div>
    </div>
    <div class="modal-footer">
        <button class="btn-cancel" >
            <span class="lbl-btn" ng-click="onCancelBtnClickHandler()">Cancel</span>
        </button>
        <button class="btn-ok" >
            <span class="lbl-btn" ng-click="onOkBtnClickHandler()">OK</span>
        </button>
    </div>
</div>