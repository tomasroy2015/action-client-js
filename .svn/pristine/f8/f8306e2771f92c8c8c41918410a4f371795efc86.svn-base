/* Right side contents section
======================================================*/
.col-md-9.right-content {
    padding : 0;
    /*width: calc(100% - 252.500px);*/

    /* Alternative of calc */
    /*margin-left:-252.500px;*/
    /*padding-left: 252.500px;*/
    /*width: 100%;*/
    /*-moz-box-sizing: border-box;*/
    /*-webkit-box-sizing: border-box;*/
    /*box-sizing: border-box;*/
    /*position:relative;*/
    /*z-index:0;*/
    /* End of alternative of calc */
}

@supports (width :  calc(100% - 252.500px)) {
    .col-md-9.right-content {
        width            : calc(100% - 252.500px);
    }
}

@supports not (width: calc(100% - 252.500px)) {
    .col-md-9.right-content {
        margin-left        : -252.500px;
        padding-left       : 252.500px;
        width              : 100%;
        -moz-box-sizing    : border-box;
        -webkit-box-sizing : border-box;
        box-sizing         : border-box;
        position           : relative;
        z-index            : 0;
    }
}

.content-page-header {
    position         : relative;
    height           : 80px;
    background-color : #878787;
    margin           : 0;
    padding          : 0;
}

.content-main {
     min-height: 91%;
     height: 91%;
    margin     : 0 auto;
    padding    : 10px;
    /*border:1px solid #FF0000;*/
}

/* Map section
======================================================*/
#worldMap, #list {
    /*border:1px solid #FFFF00 !important;*/
    min-height : 100%;
    height     : 100%;
}

#worldMap svg {
   /* border:1px solid #000088 !important;*/

    min-height : 100%;
