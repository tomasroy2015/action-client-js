

/*-----------------------------------
actionScoreBar directive style
-------------------------------------*/
.lisview-container {
    padding-top: 0px;
    width: 98%;
    margin-top: 0px;
    /*border : 1px solid deeppink;*/
    height:100%;
    min-height:100%;
}

.lisview-container .row-container{
    margin-right: 0;
    margin-left: 0;
}
.selected {
    background-color: #d6f3da;
}

 td div {
     width: 200px;
     white-space: nowrap;
     overflow: hidden;
     text-overflow: ellipsis;
 }

 .table  tbody > tr div{
     height: 40px;
 }

 .table thead > tr > th {
     vertical-align: bottom;
     border-bottom: 1px solid #000000;
 }

 #nameContainerDiv.row{
     margin-right: 0px !important;
     margin-left: 0px !important;
 }
 #tbl>tbody>tr>td>div{
     padding-top: 5px;

 }
 .nameColumnPadding{
     padding-top: 7px !important;
 }
 .scoreBarColumnPadding{
    padding-top: 8px !important;
}
span#name{
    line-height: 27px;
}
/*
#tbl>tbody>tr:hover { background: #c8c8c8 !important; }*/

#tbl>tbody>tr:hover { background: #c8c8c8 !important; }
/*header column arrow icon style*/
#tbl.ng-table #tbl.th.sortable #tbl.div:after, #tbl.ng-table #tbl.th.sortable #tbl.div:before {
    content: "";
    border-width: 0 4px 4px;
    border-style: solid;
    border-color: #000 transparent;
    visibility: visible;
    right: 110px;
    top: 30%;
    position: absolute;
    opacity: .3;
    margin-top: -4px;
}
#tbl>tbody>tr{
    padding-bottom: 0px !important;
}
#tbl>tbody>tr>td{
    padding: 0px !important;
}

#tbl.table > tbody + tbody {
    border-top: 0px solid #dddddd;
}


/*list template main div */
#parentDiv.content-main {
    min-height: 91%;
    height: 91%;
    /* margin: 0 auto; */
    margin-left: 28px;
    padding-left: 10px;
    padding-right: 15px;
    /* border: 1px solid #FF0000; */
}

