/* Activity log section
======================================================*/
.up-button-container {
  width: 100%;
  position: relative;
  margin-bottom: 10px;
  height: 18px;
}
.up-button-container > .btn-up-scroll {
  width: 18px;
  height: 18px;
  text-align: center;
  margin: 0 auto;
  display: block;
  position: relative;
}
.up-button-container > .btn-up-scroll:hover {
  border-radius: 50%;
  background: #555759;
  color: #fff;
  font-size: 0.9em;
  cursor: pointer;
}
.up-button-container > .btn-up-scroll .up-icon {
  text-align: center;
  position: absolute;
  font-size: 0.9em;
  top: 50%;
  left: 50%;
  margin-top: -.6em;
  margin-left: -.4em;
}

.down-button-container {
  position: relative;
  width: 100%;
  text-align: center;
  display: inline-block;
  height: 18px;
}
.down-button-container > .btn-down-scroll {
  width: 18px;
  height: 18px;
  text-align: center;
  margin-left: auto;
  margin-right: auto;
  display: block;
  position: relative;
}
.down-button-container > .btn-down-scroll:hover {
  border-radius: 12px;
  background: #555759;
  color: #fff;
  font-size: 0.9em;
  cursor: pointer;
}
.down-button-container > .btn-down-scroll .down-icon {
