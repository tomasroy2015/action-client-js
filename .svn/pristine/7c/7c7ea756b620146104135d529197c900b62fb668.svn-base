/* Dataset selection section
======================================================*/
.ds-collasped-panel-bottom-border {
  border-bottom: 1px solid #000;
  margin-bottom: 2px;
}

.dataset-selection {
  height: 30px;
  position: relative;
  font-family: "AkkuratStd-Regular";
  text-align: left;
  padding-left: 5px;
  color: #000;
  font-size: 11px;
  background: #c6c6c6;
  width: 200px;
}
.dataset-selection:hover {
  background: rgba(198, 198, 198, 0.4);
}
.dataset-selection:hover:before {
  border-color: transparent transparent transparent rgba(198, 198, 198, 0.4);
}
.dataset-selection:hover:after {
  border-color: transparent transparent transparent #fff;
}
.dataset-selection:after, .dataset-selection:before {
  content: "";
  display: inline-block;
  border-width: 15.5px;
  border-style: solid;
  width: 0px;
  height: 0px;
  left: 100%;
  top: 0;
  position: absolute;
  z-index: 1;
}
.dataset-selection:after {
  border-color: transparent transparent transparent #c6c6c6;
}
.dataset-selection:before {
  border-width: 15.5px;
  display: block;
  border-color: transparent transparent transparent #c6c6c6;
  z-index: 2;
  top: 0px;
}

