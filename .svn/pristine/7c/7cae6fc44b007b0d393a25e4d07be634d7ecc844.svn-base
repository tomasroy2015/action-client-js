const APPLICATION_MAIN_MODULE_NAME = 'action';
function AppModule() {
    if (!(this instanceof AppModule)) return new AppModule();
    AppModule.prototype.MAIN_MODULE = APPLICATION_MAIN_MODULE_NAME;
    AppModule.prototype.DATA_FILTER ='datafilter';
}