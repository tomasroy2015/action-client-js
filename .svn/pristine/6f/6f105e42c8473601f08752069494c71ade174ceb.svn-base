.show-in{
    opacity: 1;
    transition: opacity 3000ms ease-in;
    -webkit-transition:opacity 3000ms ease-in;
}

.show-out{
    opacity: 0;
    transition: opacity 400ms ease-out;
    -webkit-transition:opacity 400ms ease-in;
}
.fullScorecard-main-container{
    overflow: auto;
    height: 690px;
}
.fullScorecard-main-container .full-scorecard-content-container {
    width: 99%;
    height: 99%;
    position: absolute;
    top: 2px;
    overflow: auto;
}
.show-by-animation{
    -webkit-animation-duration:.4s;
    -moz-animation-duration:.4s;
    -o-animation-duration:.4s;
    animation-duration:.4s;
    -webkit-animation-name:show-details;
    -moz-animation-name:show-details;
    -o-animation-name:show-details;
    animation-name:show-details;
    -webkit-animation-iteration-count:1;
    -moz-animation-iteration-count:1;
    -o-animation-iteration-count:1;
    animation-iteration-count:1;
    -webkit-animation-timing-function:ease-in;
    -moz-animation-timing-function:ease-in;
    -o-animation-timing-function:ease-in;
    animation-timing-function:ease-in;
    -webkit-animation-fill-mode:forwards;
    -moz-animation-fill-mode:forwards;
    -o-animation-fill-mode:forwards;
    animation-fill-mode:forwards;
}
@-webkit-keyframes show-details {
    0%  {opacity: 0}
    25% {opacity: 0}
    50% {opacity: 0}
    75% {opacity: 0}
    100%{opacity: 1}
}
@-moz-keyframes show-details {
    0%  {opacity: 0}
    25% {opacity: 0}
    50% {opacity: 0}
    75% {opacity: 0}
    100%{opacity: 1}
}
@-o-keyframes show-details {
    0%  {opacity: 0}
    25% {opacity: 0}
    50% {opacity: 0}
    75% {opacity: 0}
    100%{opacity: 1}
}
@keyframes show-details {
    0%  {opacity: 0}
    25% {opacity: 0}
    50% {opacity: 0}
    75% {opacity: 0}
    100%{opacity: 1}
