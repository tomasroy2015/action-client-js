<link rel="stylesheet" href="Views/Shared/SCSS/ServiceAreaTabBarTemplate.css"/>
<div id="{{'serviceArea-' + serviceAreaId}}">
    <div class="btn-group" data-toggle="buttons">
        <label class="btn btn-primary {{ $index == 0 ? 'active' : ''}}" ng-click="onClickDir(eachSA)" ng-repeat="eachSA in serviceAreaCollection track by $index">
            <input type="radio" name="options" id="{{eachSA.QuestionId}}" autocomplete="off"
                   value="{{eachSA.QuestionId}}"> {{eachSA.QuestionId}}
        </label>

    </div>
</div>
