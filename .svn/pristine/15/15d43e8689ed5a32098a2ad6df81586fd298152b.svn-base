.vscrollbar
{
    overflow-x: hidden; /*for hiding horizontal scroll bar*/
    overflow-y: auto; /*for vertical scroll bar*/
}
/*list pagination */
.pagination {
    display: inline-block;
    border-radius: 1px;
    float: right;
    padding: 0;
    margin-bottom: 3px;
    margin-top: -10px;
}

/*Question score box style*/
.horizontal { display: inline; float: left; }
