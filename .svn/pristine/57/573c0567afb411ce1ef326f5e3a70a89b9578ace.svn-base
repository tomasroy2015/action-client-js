<div class="question-score-directive-div">
    <!-- tooltip-append-to-body="true"-->
    <svg width="27" height="25" class="question-score-directive-svg"
         tooltip-popup-delay='500' tooltip-trigger="mouseenter"
         tooltip-placement="bottom"
         tooltip-append-to-body="true"
          tooltip="{{QuestionText}}">
        <g >
            <rect height="25" width="27" x="0" y="0"></rect>
            <text  class="question-score-directive-font" x="50%" y="50%" fill="#ffffff" text-anchor="middle" dominant-baseline="central">
                {{scoreTex}}
            </text>
        </g>
    </svg>
</div>