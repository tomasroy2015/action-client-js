.content-page-header {
  position: relative;
  height: 80px;
  background-color: #878787;
  margin: 0;
  padding: 0; }

.content-main {
  min-height: 91%;
  height: 91%;
  margin: 0 auto;
  padding: 10px; }

.view-container {
  width: 100%;
  height: 100%;
  min-height: 100%; }

/*=====================================================
@Media Query Section
======================================================*/
@media only screen and (min-width: 768px) {
  .col-md-3.left-menu {
    float: left;
    /*         z-index : 50000; */ }

  .col-md-9.right-content {
    float: left;
    width: -webkit-calc(100% - 252.500px);
    width: -moz-calc(100% - 252.500px);
    width: -ms-calc(100% - 252.500px);
    width: -o-calc(100% - 252.500px);
    width: calc(100% - 252.500px); } }
@media only screen and (max-width: 767px) {
  .col-md-3.left-menu {
    float: left; }

  .col-md-9.right-content {
    float: left;
    /*width            : calc(100% - 252.500px);*/
    width: -webkit-calc(100% - 252.500px);
    width: -moz-calc(100% - 252.500px);
    width: -ms-calc(100% - 252.500px);
    width: -o-calc(100% - 252.500px);
    width: calc(100% - 252.500px);
    /*background : #ff0000;*/ } }
/*=====================================================
Slide in/out Section
======================================================*/
.slide-out {
  position: fixed;
  left: -253px;
  z-index: 20; }

.slide-in {
  position: fixed;
  top: 20px;
  left: -253px;
  width: 200px;
  height: 200px;
  background: #f00;
  z-index: 10; }

/* Full scorecard section
======================================================*/
.full-scorecard-window-container {
  width: calc(100% - 285px);
  height: 92%;
  background: #fff;
  left: -100%;
  -webkit-box-shadow: 0 0 4px 2px rgba(0, 0, 0, 0.5);
  -moz-box-shadow: 0 0 4px 2px rgba(0, 0, 0, 0.5);
  box-shadow: 0 0 4px 2px rgba(0, 0, 0, 0.5); }

.data-filter-window-container {
  width: 423px;
  height: 627px;
  background: #555759;
  left: -calc(100% - 0px);
  -webkit-box-shadow: 0 0 4px 2px rgba(0, 0, 0, 0.5);
  -moz-box-shadow: 0 0 4px 2px rgba(0, 0, 0, 0.5);
  box-shadow: 0 0 4px 2px rgba(0, 0, 0, 0.5); }

