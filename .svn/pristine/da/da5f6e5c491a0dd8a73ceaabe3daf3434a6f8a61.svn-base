/* All external import files
======================================================*/
div.service-area-questions-container {
  margin-right: 0px;
  width: 235px;
  /*question item width in left panel.total background width of a item*/
  margin-top: 10px;
  padding-left: 0px;
}

/*Question hover arrow style  */
ul.question {
  display: table;
  width: 100%;
  /*94% need to check from this line */
  padding: 0px;
  margin: 0px;
  position: relative;
  width: 235px;
}
ul.question li.questionListItem {
  display: block;
  height: 26px;
  position: relative;
  padding: 0px;
  text-align: center;
  width: 235px;
}
ul.question .questionListItemDiv {
  position: relative;
  line-height: 22px;
  /* equal to the list item's height */
  height: 22px;
  width: 222px;
  padding-left: 3px;
  float: left;
}
ul.question .questionListItemDiv:not(.selectedQuestionListItemDiv):hover {
  background-color: #ddddde;
  cursor: pointer;
}
ul.question .questionListItemDiv:not(.selectedQuestionListItemDiv):hover:after {
  top: 0;
  content: "";
  position: absolute;
  right: -11px;
  border-top: 11px solid transparent;
  border-bottom: 11px solid transparent;
  border-left: 11px solid #ddddde;
  z-index: 100;
  right: -calc(11px);
}

.summaryItemContainer {
  width: 235px;
  background: #ddddde;
  font-weight: bold;
}

.topGroupContainer {
  margin-bottom: 12px;
  font-size: 12px;
}

.bottomGroupContainer {
  margin-top: 20px;
  font-size: 12px;
}

.questionLabelDiv {
  text-align: left;
  margin-top: 0px;
  width: 120px;
  float: left;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  top: 0;
  margin-right: 3px;
  padding-left: 0px;
}

.question-score-bar {
  margin-top: 0px;
  width: 98px;
  display: inline-block;
  float: left;
  top: 0;
  font-weight: bold;
}

.scoreContainer {
  height: 24px;
  width: 95px;
  float: left;
}

.questionScoreBar {
  background-color: transparent;
  padding-left: 0;
  padding-right: 0;
  margin-left: 0;
  margin-right: auto;
  display: block;
  float: left;
  top: 5px;
}

.questionText {
  color: #000000;
}

ul li {
  list-style: none;
}

ul.question > li[data-questiontype='group']:first-child {
  margin-top: 0;
}

/*start*/
.selectedQuestionListItemDiv {
  cursor: pointer;
  background-color: #FFED00;
  position: relative;
  line-height: 22px;
  /* equal to the list item's height */
  height: 22px;
  width: 222px;
}
.selectedQuestionListItemDiv:hover {
  background-color: #FFED00;
  cursor: pointer;
}
.selectedQuestionListItemDiv:hover:after {
  top: 0;
  content: "";
  position: absolute;
  right: -11px;
  border-top: 11px solid transparent;
  border-bottom: 11px solid transparent;
  border-left: 11px solid #FFED00;
  z-index: 100;
  right: -calc(11px);
}

.selectedQuestionListItemArrow {
  border-top: calc(11px) solid transparent;
  border-bottom: calc(11px) solid transparent;
  border-left: calc(11px) solid #FFED00;
  float: left;
}

.SESHeaderContainer {
  background-color: white;
  font-weight: bold;
  width: 210px;
  padding-left: 3px;
  margin-top: 5px;
  margin-bottom: 5px;
}

.SESHeaderContent {
  border-bottom: solid 2px lightgrey;
  width: 210px;
  display: table-cell;
  vertical-align: bottom;
  text-align: left;
}

div.clear {
  clear: both;
}

