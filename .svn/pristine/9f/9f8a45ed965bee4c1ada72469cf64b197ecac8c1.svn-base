.content-page-header {
    position         : relative;
    height           : 80px;
    background-color : #878787;
    margin           : 0;
    padding          : 0;
}

.content-main {
    min-height : 91%;
    height     : 91%;
    margin     : 0 auto;
    padding    : 10px;
}

/* Map section
======================================================*/
#worldMap, #list {
    min-height : 100%;
    height     : 100%;
}

#worldMap svg {
    min-height : 100%;
    height     : 100%;
}

/* Listview section
======================================================*/
#list .row-container {
    width      : 100%;
    height     : 100%;
    min-height : 100%;
}

/*Leftmenu dataset
======================================================*/
.modal-backdrop {
    z-index    : 0 !important;
    background : none !important;
}

.modal-header {
    color : #000000;
}

.dataset-modal {
    /*position: fixed;*/
    z-index : 1200;
}

/*=====================================================
@Media Query Section
======================================================*/
@media only screen and (min-width : 768px) {
    .col-md-3.left-menu {
        float   : left;
/*         z-index : 50000; */
    }

    .col-md-9.right-content {
        float : left;
        width : -webkit-calc(100% - 252.500px);
        width : -moz-calc(100% - 252.500px);
        width : -ms-calc(100% - 252.500px);
        width : -o-calc(100% - 252.500px);
        width : calc(100% - 252.500px);
    }
}

@media only screen and (max-width : 767px) {
    .col-md-3.left-menu {
        float : left;
    }

    .col-md-9.right-content {
        float : left;
        /*width            : calc(100% - 252.500px);*/
        width : -webkit-calc(100% - 252.500px);
        width : -moz-calc(100% - 252.500px);
        width : -ms-calc(100% - 252.500px);
        width : -o-calc(100% - 252.500px);
        width : calc(100% - 252.500px);
        /*background : #ff0000;*/
    }
}


/*=====================================================
Slide in/out Section
======================================================*/

.slide-out {
    position   : fixed;
    left       : -253px;
    z-index    : 20;
}

.slide-in {
    position   : fixed;
    top        : 20px;
    left       : -253px;
    width      : 200px;
    height     : 200px;
    background : #f00;
    z-index    : 10;
}

/* Full scorecard section
======================================================*/
.full-scorecard-window-container{
    width      : 200px;
    height     : 200px;
    background : #f00;
}

