.logout-container .logout-url-container {
    right     : 60px;
    position  : absolute;
    transform : translateY(100%);
}

.logout-right-arrow-container {
    right: -24px;
    top: -15px;
    position: absolute;
    transform: translateY(85%);
}

.logout-arrow-container {
    right: -18px;
    top: -14px;
    position: absolute;
    transform: translateY(90%);
}

.logout-arrow-container:hover
{
    text-decoration : none;
    color           : #ddddde;
}
.logout-circle-arrow-size-2x {
    vertical-align: middle;
    padding-left: 10px;
    font-size: 1.5em; }
