/* Page header tab bar */
.tab-panel-container .nav {
    position:absolute;
    padding-left: 50px;
    margin-bottom: 1px;
    list-style: none;
    bottom:0;
}

.tab-panel-container .nav-tabs{
    border-bottom: none;
}

.nav-tabs > li > a {
    height           : 32px;
    margin-right     : 2px;
    line-height      : 1.42857143;
    border           : none;
    border-radius    : 0;
    background-color : #ddddde;
    color            : #000;
}

.nav-tabs > li.active > a, .nav-tabs > li.active > a:focus, .nav-tabs > li.active > a:hover {
    color               : #000;
    cursor              : default;
    background-color    : #fff;
    border              : none !important;
    border-bottom-color : transparent !important;
}

.service-area-name{
    position: absolute;
    left: 50px;
    top: 10px;
    font-size: 12px;
    font-weight: bold;
    color: #fff;
}

.service-area-bellow{
    top: 30px;
}

/*------top bar logo------*/
.top-bar-logo {
    position: absolute;
    right: 12px;
    top: 6px;
    cursor: pointer;
}
/*-------help-------*/
.help-btn {
    position: absolute;
    font-size: 11px;
    right: 155px;
    top: 35px;
    cursor: pointer;
    color: #ffffff;
}

.help-btn:hover{
    text-decoration : none;
    color           : #ddddde;
}
.help-btn > .help-circle-container {
    right: -22px;
    padding-left: 35px;
    top: -11px;
    position: absolute;
    transform: translateY(69%);
    width: 50px;
}
.help-btn > .help-circle-container:hover{
    color: #ddddde;
    opacity: 0.7;
    filter: alpha(opacity=45);
}
.question-mark-text {
    top: -14px;
    right: 5px;
    position: absolute;
    color: #555759;
    cursor: pointer;
    transform: translateY(90%);
}
.help-circle{
    margin: 0 auto;
    width:15px;
    height:15px;
    border-radius: 50%;
    background-color: #ffffff;
}
/*---------Settings-----*/
.settings-logo {
    position: absolute;
    top: 35px;
    right: 15px;
    cursor: pointer;
}
.settings-logo:hover
{
    color: #ddddde;
    opacity: 0.7;
    filter: alpha(opacity=45);
}
/*-------------Log out button------------*/
.logout-container {
    position: absolute;
    margin-top: 35px;
    height: 20px;
    right: 72px;
    cursor: pointer;
    font-size: 11px;
    font-family: "AkkuratStd-Regular";
    color: #ffffff;
}

.logout-container:hover{
    text-decoration : none;
    color           : #ddddde;
}
.header-font-size{
    font-size: 11px;
}
