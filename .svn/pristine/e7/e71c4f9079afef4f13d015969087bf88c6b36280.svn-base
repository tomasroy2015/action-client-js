.ng-modal-container.search-container{
    font-family: "AkkuratStd-Regular", "Segoe UI Light";
}
.ng-modal-container .ng-modal-overlay.search-overlay {
    /* A dark translucent div that covers the whole screen */
    position:absolute;
    z-index:10000;
    top:0;
    left:0;
    width:100%;
    height:100%;
    background-color:#ddddde;
    opacity: 0.8;
}
.ng-modal-container .ng-modal-dialog.search-dialog {
    /* A centered div above the overlay with a box shadow. */
    z-index:10000;
    position: absolute;
    width: 50%; /* Default */
    height: 30%;
    /* Center the dialog */
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    background-color: #fff;
    box-shadow: 4px 4px 80px #000;
}
.ng-modal-container .ng-modal-dialog-content.search-dialog-content {
    padding: 0px;
    text-align: left;
    height: 80%;
}
.ng-modal-container .cursor{
    cursor: pointer;
}
.ng-modal-container .fa-lg{
    font-size: 19px;
}
.ng-modal-container .ng-modal-close {
    position: absolute;
    top: 3px;
    right: 5px;
    padding: 5px;
    cursor: pointer;
    font-size: 120%;
    display: inline-block;
    font-weight: bold;
    font-family: 'arial', 'sans-serif';
}
.ng-modal-container .search-dialog .confirm-btn-container-div{
    text-align: left;
    margin-left: 37px;
    height: 20%;
}
.ng-modal-container .dialog-header.search-header{
    font-size: 16px;
    font-weight: bold;
    padding-bottom: 10px;
    min-height: 16.428571429px;
    background-color: #000000;
    color: white;
    padding-top: 15px;
    padding-right: 0px;
    padding-bottom: 15px;
    padding-left: 15px;
    /* border-bottom: 1px solid #e5e5e5; */
    height: 60px;
    display: inline-block;
    width: 100%;
}
.ng-modal-container .search-header .modal-title{
    font-size: 16px;
    font-weight: bold;
    width: 90%;
    display: inline-block;
}
.ng-modal-container .search-dialog-content .element-parent-div{
    margin-top: 10px;
    margin-left: 14px;
    position: relative;
    width: 100%;
    /* height: 70%; */
    display: flex;
    text-align: left;
    display: inline-block;
    /*margin-bottom: 15px;*/
}
.ng-modal-container .search-dialog-content .element-parent-div .element-container-div{
    height: 35px;
    width: 190px;
    margin-bottom: 22px;
}
.ng-modal-dialog-content label{
    vertical-align: top;
}
.ng-modal-dialog-content label> input[type="checkbox"] {
    margin: 0;
}
.ng-modal-dialog-content.search-dialog-content .element-parent-div .inline-block{
    display: inline-block;
}
.ng-modal-dialog-content.search-dialog-content .element-parent-div .label-width{
    width: 110px;
    padding-top: 4px;
    padding-right: 5px;
}

.ng-modal-dialog-content.search-dialog-content .element-parent-div .text-input-normal{
    width: 190px;
    height: 22px;
    border-radius: 0px;
    font-size: 12px;
    padding: 2px 2px 0px 4px;
    color: #000;
}
.ng-modal-dialog-content .drop-down{
    width: 200px;
}

.ng-modal-container .btn{
    height: 22px;
    border: none;
    border-radius: 0;
    background: #878787 !important;
    margin-left: -3px;
    padding-top: 2px;
    color: white;
    width: 65px;
    margin-right: 10px;
}
.ng-modal-container .left-align{
    float: left;
}
.ng-modal-container .right-align{
    float: right;
}
.ng-modal-container .clear{
    clear: both;
}
.input-validation
{
    border-style:solid;
    border-width: 1px;
    border-color: #ff0000;
}
.show-error{
    color: #ff0000;
    font-size: 11px;
    display: inline-block;
    margin-top: 3px;
}