html, body {
    min-height : 100%;
    height     : 100%;
}

.container-fluid, .row-fluid {
    min-height : 100%;
    height     : 100%;
    padding    : 0;
}

.col-md-3.left-menu, .col-md-9.right-content {
    min-height : 100%;
    height     : 100%;
}

.col-md-3.left-menu {
    /*     border           : 1px solid #ff0000; */
    padding            : 10px;
    color              : #efefef;
    /*     border-right: 2px solid #ddddde;     */
    /*     box-shadow: -5px 0 5px -5px #333, 5px 0 5px -5px #ff0000; */

    -webkit-box-shadow : -2px 0px 3px 0px #ddddde, 2px 0px 3px 0px #ddddde;
    -moz-box-shadow    : -2px 0px 3px 0px #ddddde, 2px 0px 3px 0px #ddddde;
    box-shadow         : -2px 0px 3px 0px #ddddde, 2px 0px 3px 0px #ddddde;
}

img.logo {
    padding : 20px !important;
}

.col-md-9.right-content {
    padding : 0;
}

.content-page-header {
    position : relative;
    height           : 80px;
    background-color : #878787;
    margin           : 0;
    padding          : 0;
}

.content-main {
    min-height : 91%;
    height     : 91%;
    margin     : 0 auto;
    padding    : 10px;
    /*border:1px solid #FF0000;*/
}

#worldMap, #list {
    /*border:1px solid #FFFF00 !important;*/
    min-height : 100%;
    height     : 100%;
}

#worldMap svg {
    /* border:1px solid #000088 !important; */
    min-height : 100%;
    height     : 100%;
}

#list .row-container {
    width      : 100%;
    height     : 100%;
    min-height : 100%;
}