/* All external import files
======================================================*/
.evaluationContainer {
  height: 100%;
}

.headerContainer {
  height: 80px;
  background-color: #ddddde;
}

.labelContainer {
  width: 380px;
  float: left;
  height: 100%;
}

.filterContainer {
  width: 310px;
  float: left;
  height: 100%;
}

.filterInput {
  padding-top: 10px;
}

.translationContainer {
  width: 300px;
  float: left;
  height: 100%;
}

.dataContainer {
  height: 750px;
}

#evaluationLabel {
  font-size: 20px;
}

.search-box, .close-icon, .search-wrapper {
  position: relative;
}

.search-input-wrapper {
  width: 200px;
  margin: auto;
  float: left;
}

.search-button-wrapper {
  float: left;
}

.search-box {
  width: 100%;
  outline: 0;
}
.search-box:not(:valid) ~ .close-icon {
  display: none;
}

.close-icon {
  border: 1px solid transparent;
  background-color: transparent;
  display: inline-block;
  vertical-align: middle;
  outline: 0;
  cursor: pointer;
  position: absolute;
  margin-left: 175px;
  margin-top: -14px;
}
.close-icon:after {
  content: "X";
  display: block;
  width: 15px;
  height: 15px;
  position: absolute;
  background-color: #FA9595;
  z-index: 1;
  top: 0;
  bottom: 0;
  margin: auto;
  padding: 1px;
  border-radius: 50%;
  text-align: center;
  color: white;
  font-weight: normal;
  font-size: 12px;
  box-shadow: 0 0 2px #E50F0F;
  cursor: pointer;
}

