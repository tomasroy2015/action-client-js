.ng-modal-container{

}
.ng-modal-container .ng-modal-overlay {
    /* A dark translucent div that covers the whole screen */
    position:absolute;
    z-index:9999;
    top:0;
    left:0;
    width:100%;
    height:100%;
    background-color:#ddddde;
    opacity: 0.8;
}
.ng-modal-container .ng-modal-dialog {
    /* A centered div above the overlay with a box shadow. */
    z-index:10000;
    position: absolute;
    width: 50%; /* Default */
    height: 65%;
    /* Center the dialog */
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    background-color: #fff;
    box-shadow: 4px 4px 80px #000;
}
.ng-modal-container .ng-modal-dialog-content {
    padding:0px;
    text-align: left;
}
.ng-modal-container .cursor{
    cursor: pointer;
}
.ng-modal-container .fa-lg{
    font-size: 19px;
}
.ng-modal-container .ng-modal-close {
    position: absolute;
    top: 3px;
    right: 5px;
    padding: 5px;
    cursor: pointer;
    font-size: 120%;
    display: inline-block;
    font-weight: bold;
    font-family: 'arial', 'sans-serif';
}
.ng-modal-container .confirm-btn-container-div{
    /*padding-top: 12px;*/
    /*padding-left: 30px;*/
    margin-right: 5px;
    text-align: center;
    /*padding-bottom: 40px;*/
}
.ng-modal-container .dialog-header{
    font-size: 16px;
    font-weight: bold;
    padding-bottom: 10px;
    min-height: 16.428571429px;
    background-color: #000000;
    color: white;
    padding-top: 15px;
    padding-right: 0px;
    padding-bottom: 15px;
    padding-left: 15px;
    /* border-bottom: 1px solid #e5e5e5; */
    height: 60px;
    display: inline-block;
    width: 100%;
}
.ng-modal-container .modal-title{
