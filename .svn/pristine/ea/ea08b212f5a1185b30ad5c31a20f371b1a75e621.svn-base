.show-in{
    opacity: 1;
    transition: opacity 1500ms ease-in;
    -webkit-transition:opacity 1500ms ease-in;
}

.show-out{
    opacity: 0;
    transition: opacity 400ms ease-out;
    -webkit-transition:opacity 400ms ease-in;
}