.dataset-heading {
    background-image: url("");
    background-color: #000 !important;
    color: #fff !important;
    height: 57px;
    line-height: 42px;
    font-size: 1.416666666666667em;
    /* 17px/12px */
    text-shadow: none;
    border-radius: 0; }
.dataset-heading .logo {
    float: right;
    margin-top: 10px;
    margin-right: 10px; }

.dataset-body-container {
    padding: 18px; color: #000000;}
.dataset-body-container .content-heading {
    font-weight: bold;
    font-size: 1em;
    font-weight: bold;
    padding-left: 10px;
    color: #000000;
}
.dataset-body-container #ddlClient {
    margin-left: 20px;
    padding-left: 3px;
    width: 180px;
    height: 24px;
    border-radius: 0;
    font-weight: normal; }

input[type='radio'] {
    margin-top: 0; }

#ddlMonthSelect input[type='radio'], #ddlQuarterSelection input[type='radio'] {
    margin-left: 5px;
    margin-top: 0;
    padding-left: 3px;
    width: 180px;
    height: 24px;
    border-radius: 0;
    font-weight: normal; }

.btn-group > div {
    margin-bottom: 5px; }
.btn-group > div#monthDdlContainer, .btn-group > div#quarterDdlContainer {
    display: flex; }

select#ddlMonthSelect {
    width: 150px;
    height: 24px;
    margin-left: 20px; }

select#ddlQuarterSelection {
    width: 150px;
    height: 24px;
    margin-left: 14px; }

.checkbox-panel {
    margin-top: 20px;
    padding-left: 25px;
    padding-right: 25px;
    color: #000000;
}

.button-container {
    position: relative;
    width: 100%;
    height: 40px;
    padding-top: 10px;}

.btn-white-bg {
    padding-top: 0;
    height: 26px;
    line-height: 26px;
    background-color: #555759;
    color: #fff;
    border-radius: 0; }
.btn-white-bg:hover {
    background-color: rgba(85, 87, 89, 0.8); }

.right-side {
    position: absolute;
    right: 20px; }

#datePanel {
    margin-top: 10px; }

.form-horizontal .control-label {
    text-align: left; }
