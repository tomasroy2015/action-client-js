.question{
    display: table;
    width: 100%;/*94% need to check from this line */
    padding: 0px;
    margin: 0px;
    position: relative;

}
.questionListItem{
    display: block;
    height: 25px;
    position: relative;
    padding: 0px;
    margin: 0px;
    text-align: center;
    border: 1px solid transparent;
}
.questionListItem:hover{
    background-color: #c9c9c9;
    cursor: pointer;
}
.questionListItemDiv{
    position:relative;
    line-height: 25px; /* equal to the list item's height */
    height:100%;
    width: 100%;
}
.questionListItemDiv:after,.questionListItemDiv:before{
    content:"";
    display:inline-block;
    border-width: 13px;
    border-style: solid;
    width: 0px;
    height: 0px;
    left: 100%;
    top: -1px;
    position: absolute;
    z-index: 1;
}
.questionListItemDiv:after{
    border-color: transparent transparent transparent transparent;
}
.questionListItem:hover .questionListItemDiv:before{
    border-color:transparent transparent transparent #c9c9c9;
}
.questionListItemDiv:before{
    border-width: 13px; /*arrow width. for changing arrow width, change this value  */
    display: block;
    border-color: transparent transparent transparent transparent;
    z-index: 2;
    top:-1px;/* arrow top position.for changing arrow icon top position need change this value*/
}
/*start*/
.selectedQuestionListItemDiv{
    position:relative;
    line-height: 25px; /* equal to the list item's height */
    height:100%;
    width: 100%;
}
.selectedQuestionListItemDiv:after,.selectedQuestionListItemDiv:before{
    content:"";
    display:inline-block;
    border-width: 13px;
    border-style: solid;
    width: 0px;
    height: 0px;
    left: 100%;
    top: -1px;
    position: absolute;
    z-index: 1;
}
.selectedQuestionListItemDiv:after{
    border-color: transparent transparent transparent transparent;
}
.selectedQuestionListItemDiv:hover .selectedQuestionListItemDiv:before{
    border-color:transparent transparent transparent #fff000;
}
.selectedQuestionListItemDiv:before{
    border-width: 13px; /*arrow width. for changing arrow width, change this value  */
    display: block;
    border-color: transparent transparent transparent transparent;
    z-index: 2;
    top:-1px;/* arrow top position.for changing arrow icon top position need change this value*/
}
/*end*/
.selectedQuestionItem>div:before {
    border-color: transparent transparent transparent #fff000;
}
.selectedQuestionItem  {
    background-color: #fff000;
    cursor: pointer;
}
.selectedQuestionItem:hover{
    background-color: #fff000;
    cursor: pointer;
}
.selectedQuestionItem>div:before:hover {
    border-color: transparent transparent transparent #fff000;
}

