html, body {
    min-height : 100%;
    height     : 100%;
}

/* Main container section
======================================================*/
.container-fluid, .row-fluid {
    min-height : 100%;
    height     : 100%;
    padding    : 0;
}

.col-md-3.left-menu, .col-md-9.right-content {
    min-height : 100%;
    height     : 100%;
}

/* Left menu section
======================================================*/
.col-md-3.left-menu {
    padding            : 10px;
    color              : #efefef;
    /*     border-right: 2px solid #ddddde;     */
    /*     box-shadow: -5px 0 5px -5px #333, 5px 0 5px -5px #ff0000; */

    -webkit-box-shadow : -2px 0px 3px 0px #ddddde, 2px 0px 3px 0px #ddddde;
    -moz-box-shadow    : -2px 0px 3px 0px #ddddde, 2px 0px 3px 0px #ddddde;
    box-shadow         : -2px 0px 3px 0px #ddddde, 2px 0px 3px 0px #ddddde;

    width              : 252.500px;
    min-width          : 252.500px;
    max-width          : 252.500px;
    position           : relative;
    z-index            : 10;

    overflow-y: auto;
    overflow-x: hidden;
}

.logo-container {
    width         : 100%;
    margin-bottom : 10px;
}

img.logo {
    width  : 233px;
    height : 60px;
    margin : 0 auto;
}

/* Right side contents section
======================================================*/
.col-md-9.right-content {
    padding : 0;
    /*width: calc(100% - 252.500px);*/

    /* Alternative of calc */
    /*margin-left:-252.500px;*/
    /*padding-left: 252.500px;*/
    /*width: 100%;*/
    /*-moz-box-sizing: border-box;*/
    /*-webkit-box-sizing: border-box;*/
    /*box-sizing: border-box;*/
    /*position:relative;*/
    /*z-index:0;*/
    /* End of alternative of calc */
}

@supports (width :  calc(100% - 252.500px)) {
    .col-md-9.right-content {
        width            : calc(100% - 252.500px);
    }
}

@supports not (width: calc(100% - 252.500px)) {
    .col-md-9.right-content {
        margin-left        : -252.500px;
        padding-left       : 252.500px;
        width              : 100%;
        -moz-box-sizing    : border-box;
        -webkit-box-sizing : border-box;
        box-sizing         : border-box;
        position           : relative;
        z-index            : 0;
    }
}

.content-page-header {
    position         : relative;
    height           : 80px;
    background-color : #878787;
    margin           : 0;
    padding          : 0;
}

.content-main {
    min-height : 91%;
    height     : 91%;
    margin     : 0 auto;
    padding    : 10px;
    /*border:1px solid #FF0000;*/
}

/* Map section
======================================================*/
#worldMap, #list {
    /*border:1px solid #FFFF00 !important;*/
    min-height : 100%;
    height     : 100%;
}

#worldMap svg {
    /* border:1px solid #000088 !important; */
    min-height : 100%;
    height     : 100%;
    border     : 1px solid red;
}

/* Listview section
======================================================*/
#list .row-container {
    width      : 100%;
    height     : 100%;
    min-height : 100%;
}

