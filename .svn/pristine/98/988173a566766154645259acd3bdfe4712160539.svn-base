<action-score-box
        ng-controller="serviceAreaScoreBoxController"
        general-survey-collection="generalSurveyCollection"
        trigger-survey-collection="triggerSurveyCollection"
        score-data-collection="combinedScoreDataCollection"
        score-bar-width="57"
        selected-question-id="selectedQuestionId"
        selected-service-area-id="selectedServiceAreaId"
        on-tab-item-click="onServiceAreaClicked(selectedId)"
        on-question-item-click="onQuestionItemClick(selectedItem)">
</action-score-box>
