.list-wrapper ul {
  margin        : 0;
  padding       : 0;
  //border        : 1px solid #000;
  border-bottom : none;
  li {
    /*  list-style:none;
     margin:0;
    padding:0; */
    list-style  : none;
    //border-bottom : 1px solid #000;
    width       : 100%;
    height      : 20px;

    padding-top : 2px;
    &:hover {
      background : #C6C6C6;
    }
    &:hover .delete {
      display : inline-block;
    }
    &.list-item > span.label {
      margin-left : 5px;
      width       : 88%;
      /* color:red; */
      display     : inline-block;
      color       : #000;
      text-align  : left;
      font-size:.9em;
    }
  }
}

