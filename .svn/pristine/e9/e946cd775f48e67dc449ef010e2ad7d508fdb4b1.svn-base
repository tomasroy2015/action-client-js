}
.open-popup-animation{
    -webkit-animation-duration:.3s;
    -moz-animation-duration:.3s;
    -o-animation-duration:.3s;
    animation-duration:.3s;
    -webkit-animation-name:open-popup-animation;
    -moz-animation-name:open-popup-animation;
    -o-animation-name:open-popup-animation;
    animation-name:open-popup-animation;
    -webkit-animation-iteration-count:1;
    -moz-animation-iteration-count:1;
    -o-animation-iteration-count:1;
    animation-iteration-count:1;
    -webkit-animation-timing-function:ease-out;
    -moz-animation-timing-function:ease-out;
    -o-animation-timing-function:ease-out;
    animation-timing-function:ease-out;
    -webkit-animation-fill-mode:forwards;
    -moz-animation-fill-mode:forwards;
    -o-animation-fill-mode:forwards;
    animation-fill-mode:forwards;
    display: block;
    /*position:absolute;*/
}
@-webkit-keyframes open-popup-animation {
    0%   {top: 40%;}
    25%  {top: 30%;}
    50%  {top: 20%;}
    100% {top: 11%; }
}
@-moz-keyframes open-popup-animation {
    0%   {top: 40%;}
    25%  {top: 30%;}
    50%  {top: 20%;}
    100% {top: 11%; }
}
@-o-keyframes open-popup-animation {
    0%   {top: 40%;}
    25%  {top: 30%;}
    50%  {top: 20%;}
    100% {top: 11%; }
}
@keyframes open-popup-animation {
    0%   {top: 40%;}
    25%  {top: 30%;}
    50%  {top: 20%;}
    100% {top: 11%; }
}


