.show-in{
    opacity: 1;
    transition: opacity 3000ms ease-in;
    -webkit-transition:opacity 3000ms ease-in;
}

.show-out{
    opacity: 0;
    transition: opacity 400ms ease-out;
    -webkit-transition:opacity 400ms ease-in;
}
