#list-view-wrapper {
    content: " ";
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 1;
    top: 80px;
    left: 0;
    background: #000;
    background: rgba(0,0,0,0.1);
    display: none;
}
.summary-popup {
    position: fixed;
    z-index: 1;
    display: none;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
    outline: 0;
    background-color: #ffffff;
    border: 1px solid #d3d3d3;
    box-shadow: 1px 1px 1px #d3d3d3;
    -webkit-box-shadow: 1px 1px 1px #d3d3d3;
    -moz-box-shadow: 1px 1px 1px #d3d3d3;;
}
.show-popup{
    width: 274px;
    left: 280px;
    top: 100px;
    display: block;
    max-height: 810px;
    position: fixed;
    z-index: 1;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
    outline: 0;
    background-color: #ffffff;
    /*border: 1px solid #d3d3d3;*/
    -webkit-box-shadow: 0 0 5px 2px #878787;
    -moz-box-shadow: 0 0 5px 2px #878787;
    box-shadow: 0 0 5px 2px #878787;
}
.hide-popup{
    display: none;
}
.summary-popup-header{
    min-height: 16px;
    /* margin-left: 2px; */
    /* padding: 2px 2px 2px 2px; */
    padding-top: 2px;
    padding-left: 14px;
    height: 65px;
    border-bottom: 1px solid #e5e5e5;
    background-color: black;
    color: white;
}
.summary-popup-header .close {
    margin: 8px 12px 0px 0px;
    opacity: 1;
    background-color: white;
    border-radius: 50%;
    width: 15px;
    height: 15px;
    font-size: 15px;
}
.summary-popup-header .close:hover{
    background-color: #d3d3d3;
}
.summary-popup-title{
    font-size: 16px;
    font-weight: bold;
    height: 26px;
    margin-top: 5px;
    width: 226px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.summary-popup-dialog{
    width: 100%;
    /*height: 94.5%;*/
    margin: 2px;
    border-right: 2px solid #d3d3d3;
}
.summary-popup-content{
    width: 100%;
    /*height: 100%;*/
    border: none;
    border-radius: 2px;
    font-family: "AkkuratStd-Regular";
    border-right: 2px solid #d3d3d3;
}
.summary-popup-body{
    position: relative;
    padding-left: 10px;
    padding-top: 18px;
    /*min-height: 350px;*/
}
.modal-map-view{
    width: 560px;
    height: 578px;
}

.summary-popup-footer{
