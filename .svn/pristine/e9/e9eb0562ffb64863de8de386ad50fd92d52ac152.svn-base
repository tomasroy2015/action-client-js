.dataset-heading {
    background-image: url("");
    background-color: #000 !important;
    color: #fff !important;
    height: 57px;
    line-height: 42px;
    font-size: 1.416666666666667em;
    /* 17px/12px */
    text-shadow: none;
    border-radius: 0; }
.dataset-heading .logo {
    float: right;
    margin-top: 10px;
    margin-right: 10px; }
.container{
    background-color: #ffffff;
}
.container.dataset-body-container {
    padding: 14px;
    color: #000000;
    font-family: "AkkuratStd-Regular";
    width: 350px;
    /*height: 380px;*/
    border-radius: 2px;
    box-shadow: 2px 2px 2px #d3d3d3;
}
.dataset-body-container .content-heading {
    font-weight: bold;
    font-size: 14px;
    color: #000000;
    border-bottom: 1px solid #000000;
}
.dataset-body-container #ddlClient {
    margin-left: 20px;
    padding-left: 3px;
    width: 180px;
    height: 24px;
    border-radius: 0;
    font-weight: normal; }

input[type='radio'] {
    margin-top: 0; }

#ddlMonthSelect input[type='radio'], #ddlQuarterSelection input[type='radio'] {
    margin-left: 5px;
    margin-top: 0;
    padding-left: 3px;
    width: 180px;
    height: 24px;
    border-radius: 0;
    font-weight: normal; }

.btn-group > div {
    margin-bottom: 5px; }
.btn-group > div#monthDdlContainer, .btn-group > div#quarterDdlContainer {
    display: flex; }

select#ddlMonthSelect {
    width: 150px;
    height: 24px;
    margin-left: 20px; }

select#ddlQuarterSelection {
    width: 150px;
    height: 24px;
    margin-left: 14px; }

.checkbox-panel {
    margin-top: 14px;
    padding-left: 25px;
    padding-right: 25px;
    color: #000000;
}

.button-load-container{
    color: #ffffff;
}

.btn-load-ok {
    position: absolute;
    text-align: center;
    left: 35%;
    height: 26px;
    border: none;
    border-radius: 0;
    margin-top: 16px;
    vertical-align: bottom;
    color: #fff;
    background: #878787 !important;
}

.btn-load-ok:hover {
    background : #555759 !important;
}

.date-panel {
    margin-left: 14px;
    height: 70px;
}

.form-horizontal{
    border-bottom: 2px solid #d3d3d3;
}

.form-horizontal .control-label {
    text-align: right;
    font-family: "AkkuratStd-Regular";
    font-weight: normal;
    font-size: 11px;
}

.form-horizontal .radio, .form-horizontal .checkbox {
    min-height: 27px;
    font-weight: normal;
    font-size: 11px;
}
#datasetSelectionGroup{
    margin-left: 10px;
    margin-top: -5px;
}

/*-------------------------------------------------
                Media Query Section
-------------------------------------------------*/
@media only screen and (max-height : 1024px) {
    .container.dataset-body-container {
        height: 380px;
    }
}

/*-------------------------------------------------
            End of Media Query Section
-------------------------------------------------*/
