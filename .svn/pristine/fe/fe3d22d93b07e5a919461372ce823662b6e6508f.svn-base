
.scorebar-mainDiv {
    padding        : 0;
    margin         : 0;
    /*     width   : 152px; */
    width          : 100%;
    height         : 25px;
    line-height    : 25px;
    float          : left display : inline-block;
    /*     border:1px solid red; */
    /*     height:100%; */
    vertical-align : middle;
    font-size      : 0.9em;
    margin-left    : 0px;
}

.scorebar-scoreDiv {
    margin-top     : 0px;
    width          : 34px;
    max-width      : 34px;
    display        : inline-block;
    /*float         : left;*/
    /*     padding-top   : 2px !important; */
    /*     text-align    : start; */
    padding-right  : 2px;
    /*      border:1px solid #000; */
    vertical-align : middle;

}

.scorebar-scorebarCanvas {
    padding-left   : 0;
    padding-right  : 0;
    margin-left    : auto;
    margin-right   : auto;
    display        : inline-block;
    /*float         : left;*/

    /*     margin-top    : 2px; */
    /*      border:1px solid #0DED33; */
    vertical-align : middle;
    margin-top     : -5px;
    padding-top    : 0;
}

.customizable-scorebar-score-label1 {
    /*      padding-top  : 7px !important; */
    text-align  : right;
    font-size   : 12px;
    font-weight : bold;
    /*     width         : 35px; */
}

.customizable-scorebar-score-label {
    /*     padding-left : 10px !important; */
    /*     padding-top  : 7px !important; */
    text-align   : start;
    padding-left : 2px;
    /*     border:1px solid #FD0; */
    /*     vertical-align:middle; */
    display      : inline; /*added by shahin oct 10-2015 */
}

/*list view scorebar css */
.list-view-scorebar-mainDiv {
    padding : 0;
    margin  : 0;
    width   : 100%;
    float   : left
}

.list-view-scorebar-scoreDiv {
    margin-top    : 0px;
    width         : 50px;
    display       : inline-block;
    float         : left;
    padding-top   : 2px !important;
    text-align    : end;
    padding-right : 5px;
}

.list-view-scorebar-scorebarCanvas {
    padding-left  : 0;
    padding-right : 0;
    margin-left   : auto;
    margin-right  : auto;
    display       : block;
    float         : left;
    margin-top    : 2px;
}