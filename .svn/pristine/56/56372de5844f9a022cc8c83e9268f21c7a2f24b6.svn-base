.modal {
  overflow-y: hidden;
  text-align: center;
}

.modal:before {
  display: inline-block;
  vertical-align: middle;
  content: " ";
  height: 100%;
}

.modal-dialog {
  display: inline-block;
  text-align: left;
  vertical-align: middle;
}

.evaluationSummaryContainer {
  height: 440px;
  width: 100%;
  background-color: #dddddd;
}
.evaluationSummaryContainer .equal-circle {
  display: inline-block;
  width: 15px;
  height: 15px;
  border-radius: 8px;
  font-size: 12px;
  color: #000;
  line-height: 15px;
  text-align: center;
  background: #FFED00;
  padding: 2px;
  margin-right: 5px;
  font-family: AkkuratStd-Regular;
}
.evaluationSummaryContainer .regular-remarks-equality {
  position: absolute;
  right: 12px;
  top: 30px;
  z-index: 1;
}
.evaluationSummaryContainer .additional-remarks-equality {
  position: absolute;
  right: 12px;
  top: 140px;
  z-index: 1;
}

.evaluationSummaryHeader {
  width: 100%;
  height: 20px;
  background-color: #000000;
  color: white;
  text-align: center;
  padding-top: 2px;
}

.btn.ac-btn-right-evaluation-summary {
  height: 18px;
  font-size: 0.9em;
  position: absolute;
  left: 350px;
  background: black;
  color: transparent;
  padding: 1px 0px 0px 5px;
  border-radius: 0;
  box-shadow: none;
  width: 50px;
}

.btn.ac-btn-left-evaluation-summary {
  height: 18px;
  font-size: 0.9em;
  position: absolute;
  left: 175px;
  background: black;
  color: transparent;
  padding: 1px 0px 0px 5px;
  border-radius: 0;
  box-shadow: none;
  width: 50px;
}

.circle-arrow-right {
  position: absolute;
  top: 2px;
  right: 175px;
  font-size: 1.4em;
  color: white;
}

.circle-arrow-left {
  position: absolute;
  top: 2px;
  left: 175px;
  font-size: 1.4em;
  color: white;
}

.btn-close-evaluation-summary {
  font-size: 1.4em;
  color: white;
  left: 580px;
  position: absolute;
  top: 2px;
}

.span-evaluation-count-container {
  position: relative;
  width: auto;
}

.headerContentContainer {
  align-self: center;
  width: 90%;
  margin: auto;
  color: white;
}

.questionListContainer {
  width: 45%;
  float: left;
}

.questionListHeader {
  margin: 10px 0px 0px 15px;
  font-size: 16px;
  height: auto;
  width: 250px;
  max-width: 250px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.questionListDetails {
  background-color: white;
  height: 375px;
  margin: 3px 0px 5px 15px;
}
.questionListDetails thead {
  flex-direction: column;
  align-items: stretch;
  display: inline-block;
  width: 250px;
}
.questionListDetails tbody {
  overflow-y: auto;
  height: 320px;
  display: inline-block;
  width: 250px;
}
.questionListDetails .table > thead > tr > th {
  vertical-align: middle;
  border: 0px;
  padding: 5px;
}
.questionListDetails .table > tbody > tr > td, .questionListDetails .table > tfoot > tr > td {
  padding: 5px;
  line-height: 12px;
  vertical-align: middle;
  border: 0px;
}

.evaluationSummaryContent {
  width: 100%;
  height: 95%;
}

.othersInfoContainer {
  width: 55%;
  float: left;
  padding-left: 15px;
  padding-right: 15px;
}
.othersInfoContainer .regularRemarksContainer {
  height: 20px;
  line-height: 20px;
  vertical-align: middle;
  width: 200px;
  text-overflow: ellipsis;
  white-space: nowrap;
  float: left;
}
.othersInfoContainer .divLabelContainer {
  height: 20px;
  line-height: 20px;
  vertical-align: middle;
  width: 200px;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.regularRemarksHeader {
  height: 20px;
  vertical-align: bottom;
  margin-top: 14px;
  text-overflow: ellipsis;
}

.additionalRemarksHeader {
  height: 20px;
  vertical-align: bottom;
  margin-top: 10px;
  text-overflow: ellipsis;
}

.regRemarksContainer {
  background-color: white;
  height: 80px;
  font-size: 10px;
  overflow: auto;
  padding: 5px;
}

.onlyRegularRemarks {
  height: 190px;
}

.additionalRemarksWrapper {
  height: 100px;
}

.adiRemarksContainer {
  background-color: white;
  height: 80px;
  font-size: 10px;
  overflow: auto;
  padding: 5px;
}

.attributeListArea {
  height: 185px;
  margin-top: 10px;
}

.attributeValueContainer {
  background-color: white;
  height: 145px;
  overflow: auto;
  overflow-y: auto;
  overflow-x: hidden;
}

.headerLabel {
  vertical-align: bottom;
  float: left;
  width: 200px;
  line-height: 20px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.translationDiv {
  float: right;
  margin-right: 10px;
}
.translationDiv .btn {
  width: 30px;
  height: 16px;
  line-height: 16px;
  padding: 0;
  margin: 0px 0px 0;
  font-size: 1em;
  font-weight: bold;
  color: black;
  border: 1px solid transparent;
  border-radius: 0;
  box-shadow: none;
  background-color: lightgrey;
}
.translationDiv .selected {
  background: #ffffff;
  border-bottom: 1px solid white;
  color: #000000;
}
.translationDiv .selected :hover {
  border: 1px solid #ccc;
}

.attributeLabel {
  font-weight: bold;
}

.attributeValue {
  width: 140px;
}

.truncatedText {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.attributeList {
  display: table;
  width: 100%;
  padding: 0px;
  margin: 0px;
  position: relative;
}

.columnLeft {
  float: left;
  width: 50%;
  padding: 3px;
}

.columnRight {
  float: right;
  width: 50%;
}

.attributeItemWrapper {
  margin-bottom: 5px;
}

.labelColumn {
  width: 200px;
}

.evaluationSummaryDataContainer {
  border-bottom: 1px solid black;
  height: 30px;
  padding-top: 4px;
  width: 180px;
}
.evaluationSummaryDataContainer .questionNumber {
  font-weight: bold;
  font-size: 14px;
  float: left;
}
.evaluationSummaryDataContainer .questionText {
  float: left;
  font-size: 10px;
  margin-left: 5px;
  width: 150px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.evaluationSummaryScoreBoxContainer {
  width: 40px;
  padding-left: 10px;
  border-bottom: 1px solid black;
  height: 30px;
}

.evaluationSummaryQuestionHeader {
  border-bottom: 1px solid black;
  height: 30px;
  padding-top: 4px;
  width: 180px;
}

.evaluationSummaryScoreHeader {
  width: 40px;
  border-bottom: 1px solid black;
  height: 30px;
}

.evaluationSummaryHeaderFontSize {
  font-size: 12px;
}

.evaluationSummaryDataFontSize {
  font-size: 10px;
}

.scrollContainerWrapper {
  background-color: white;
  padding: 2px;
}

.containerBorder {
  border: 1px solid #ccc;
}

.tab {
  float: right;
}

.tab label {
  background: #eee;
  padding: 4px 4px 2px 4px;
  border: 1px solid #ccc;
  margin-left: -2px;
  position: relative;
  margin-bottom: 0px;
  display: inline;
  top: 4px;
  /* left: 0px; */
}

.tab [type=radio] {
  display: none;
}

