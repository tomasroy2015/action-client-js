.view-by-attribute-root{
}
#viewByAttributeParentDiv {
    margin: 0;
    padding: 0;
}
#atttibuteItemRoot.navbar-nav>li>.dropdown-menu{
    /*margin-top: -55px;*/
    /*margin-left: -94px;*/
    /*border-radius: 0 !important;*/
    /*border: 1px solid #000000;*/
    /*font-size: 11px;*/
    /*padding-bottom: 21px;*/

    margin-top: -60px;
    margin-left: -23px;
    border-radius: 0 !important;
    border: 1px solid #000000;
    font-size: 11px;
    padding-bottom: 21px;
}
#atttibuteItemRoot.navbar-nav>.open>ul.dropdown-menu.multi-column.columns-2>.row{
    margin-right: 40px;
    margin-left: -30px;
}
#atttibuteItemRoot.navbar-nav>.open>ul.dropdown-menu.multi-column.columns-2>.row .fa{
    color: #555759;
}
.view-by-attribute-root .attributeHoverEffect>a:hover{
    background-color: #fff000 !important;
}
.view-by-attribute-root .selectedAttributeItem{
    background-color: #fff000 !important;
    margin-left: 10px;
}
.content {
    margin-left: 45px;
    padding-left: 0px;
}
.content.white {
    background :#ffffff;
    box-shadow: none;
}
.content h1 {
    font-weight: 400;
    text-transform: uppercase;
    margin: 0;
    color: #fff;
}
.content h2 {
    font-weight: 400;
    text-transform: uppercase;
    color: #333;
    margin: 0 0 20px;
}

.view-by-attribute-root .attributeGroupItem{
    padding-top: 10px !important;
}

.content p {
    font-size: 1em;
    font-weight: 300;
    line-height: 1.5em;
    margin: 0 0 20px;
}
.content p:last-child {
    margin: 0;
}
.content a.button {
    display: inline-block;
    padding: 10px 20px;
    background: #ff0;
    color: #000;
    text-decoration: none;
}
.content a.button:hover {
    background: #000;
    color: #ff0;
}
.content.title {
    position: relative;
    background: #333;
}
.content.title h1 span.demo {
    display: inline-block;
    font-size: .5em;
    padding: 10px;
    background: #fff;
    color: #333;
    vertical-align: top;
}
.content.title .back-to-article {
    position: absolute;
    bottom: -20px;
    left: 20px;
}
.content.title .back-to-article a {
    padding: 10px 20px;
    background: #f60;
    color: #fff;
    text-decoration: none;
}
.content.title .back-to-article a:hover {
    background: #f90;
}
.content.title .back-to-article a i {
    margin-left: 5px;
}

.content.black {
    background: #000;
}
.content.black p {
    color: #999;
}
.content.black p a {
    color: #08c;
}
.view-by-attribute-root .dropdown-menu {
    min-width: 200px;
}
.view-by-attribute-root .dropdown-menu.columns-2 {
    min-width: 444px;
}
.view-by-attribute-root .dropdown-menu.columns-3 {
    min-width: 478px;
}
.view-by-attribute-root .group-padding{
    padding-top: 10px;
    padding-bottom:10px ;

}
.view-by-attribute-root .dropdown-menu li a {
    padding: 2px 2px;
    font-weight: 300;
    margin-bottom: 3px;
}
.view-by-attribute-root .dropdown-menu .divider {
    height: 1px;
    margin: 9px 0;
    overflow: hidden;
    background-color: #000000;
}
.view-by-attribute-root .multi-column-dropdown {
    list-style: none;
    width: 230px;
}
.view-by-attribute-root .multi-column-dropdown-group-item-margin{
    margin-top: 15px;
}
.view-by-attribute-root .multi-column-dropdown li a {
    display: block;
    clear: both;
    line-height: 1.428571429;
    color: #333;
    white-space: normal;
}
.view-by-attribute-root .multi-column-dropdown li a:hover {
    text-decoration: none;
    color: #262626;
    background-color: #f5f5f5;
}
#atttibuteItemRoot.nav > li > a {

    margin-left: -22px;
}
.view-by-attribute-root .viewby-label{
    float: left;
    padding-bottom: 0;
    padding-top: 0;
    padding-left: 0;
    padding-right: 0;
    margin: 0;
    font-size: 18px;
    line-height: 20px;
    color: #777777 !important;
}

#selectedAttribute{
    font-size: 18px;
    color: #000000;
    font-weight:bold ;
    opacity: 1;
}
.view-by-attribute-root .view-by-attribute-open-caret {
    display: inline-block;
    width: 0;
    height: 0;
    margin-left: 6.5px;
    vertical-align: middle;
    border-top: 4px solid;
    border-right: 4px solid transparent;
    border-left: 4px solid transparent;
    color: #000000;
}
.view-by-attribute-root #selectedAttribute .fa{
    transform: rotate(90deg);
    color: #555759;
}
.view-by-attribute-root .viewby-label .fa{
    transform: rotate(270deg);
    margin-left: 5.5px;
    vertical-align: top;
}

.view-by-attribute-root .navbar {
    position: relative;
    min-height: 50px;
    margin-bottom: 4px;
    /* border: 1px solid transparent; */
}
.view-by-attribute-root .navbar-header {
    background-color: transparent;
}
.view-by-attribute-root .navbar-default {
    background-color:transparent !important;
     border-color: transparent !important;
}
.view-by-attribute-root .navbar-default .navbar-nav > .open > a, .navbar-default .navbar-nav > .open > a:hover, .navbar-default .navbar-nav > .open > a:focus {
     color: #555555;
     background-color: transparent !important;
}
.view-by-attribute-root .nav .open > a, .nav .open > a:hover, .nav .open > a:focus {
    background-color: transparent !important;
    border-color: transparent !important;
}
.view-by-attribute-root .default-cursor{
    cursor: none;
}
.dropdown-menu #attributeType2{
    width: 50%;
}
.dropdown-menu #attributeType1{
    width: 50%;
}
@media (max-width: 768px) {
    .view-by-attribute-root .dropdown-menu.multi-column {
        min-width: 240px !important;
        overflow-x: hidden;
    }
    #atttibuteItemRoot.nav > li > a {
        margin-left: 0px;
    }

}

@media (max-width: 480px) {
    .content {
        width: 90%;
        margin: 50px auto;
        padding: 10px;
    }
    #atttibuteItemRoot.nav > li > a {

        margin-left: 0px;
    }
}

/*new implementation for radio button*/
.view-by-attribute-root input.radio:empty {
    margin-left: -999px;
}
.view-by-attribute-root input.radio:hover:not(:checked) ~ label {
    color: black;
    background-color: yellow;
}
.view-by-attribute-root input.radio:checked ~ label {
    color: #777;
    background-color: yellow;
}
/* Added by Shahin [29th June-2015] */
.view-by-attribute-root .disabled-group{
    opacity: 0.3;
    cursor : not-allowed;
    pointer-events: none;
}

.view-by-attribute-root .cursor-not-allowed{
    cursor : not-allowed;
}