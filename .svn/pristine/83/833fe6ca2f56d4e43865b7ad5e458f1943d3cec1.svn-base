<link rel="stylesheet" href="Views/Shared/CSS/ScoreBarTemplate.css"/>
<div class="list-view-scorebar-mainDiv">
    <div class="list-view-scorebar-scoreDiv"><span style="font-weight: bold;font-size: 16px;">{{scoreComparator.Score}}</span></div>
    <canvas id="{{::canvasid}}" height="20" width="100" class="list-view-scorebar-scorebarCanvas"></canvas>
    <div class="list-view-scorebar-scoreDiv"
         style="padding-left: 5px !important;padding-top: 7px !important;text-align: start;padding-left: 2px"><span>{{scoreComparator.ComparatorValue}}</span></div>
</div>
