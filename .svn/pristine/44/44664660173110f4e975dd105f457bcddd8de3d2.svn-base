.country-mapping-container{
    /*padding: 5px;*/
    /*margin: 0;*/
    /*padding-left: 16px;*/
}
.country-mapping-container .template-header{
    display: inline-block;
    font-size: 14px;
    font-weight: bold;
    padding-top: 10px;
}
.country-mapping-container .table{
    width: 95%;
    display: table;
}

.country-mapping-container .table>thead,.country-mapping-container .table>tbody{
    float: left;width: 100%;
}
.country-mapping-container .table>tbody{
    overflow: auto;
    height: 380px;
}
.country-mapping-container .table>tbody>tr>td{
    padding: 2px;
    border-top: transparent;
    border-bottom:1px solid #000000;
    font-size: 11px;
    padding-left: 8px;
    overflow: hidden; white-space: nowrap;
    text-overflow: ellipsis;
}
.country-mapping-container .invisiable{
    visibility: hidden;
}
.country-mapping-container .fa-times{
    font-size: 14px;
}
.country-mapping-container .fa-times-circle{
    font-size: 14px;
}
.country-mapping-container .country-column{
    width: 150px;
    max-width: 150px;
    font-weight: bold;
}
.country-mapping-container .display-label-column{
    width: 150px;
    max-width: 150px;
    font-weight: bold;
}
.country-mapping-container .mapped-value-column{
    width: 175px;
}
.country-mapping-container .mapped-value-text{
    font-size: 10px;
    display: inline-block;
    background-color: #d3d3d3;
}
.country-mapping-container .btn-edit{
    height: 16px;
    border: none;
    border-radius: 0;
    background: inherit !important;
    color: #000000;
    width: 33px;
    margin-right: 4px;
}
.country-mapping-container .btn-edit:hover{
    background: #555759 !important;
    color: white;
}

.country-mapping-container .table>tbody>tr:hover{
    background-color: #ddddde;
}

.country-mapping-container .table>tbody>tr{
    width:100%;height:13px !important;overflow:hidden;
    background-color: #FFFFFF;
    display: table;
}
.country-mapping-container .table > thead > tr > th{
    vertical-align: bottom;
    border-bottom: 1px solid #000000;
    padding: 2px;
    font-size: 11px;
    font-weight: bold;
    padding-left: 8px;
}
.country-mapping-container .table-container{
    padding-top: 10px;
}
.country-mapping-container .footer-container-div{
    padding-top: 14px;
    padding-left: 12px;
    padding-right: 10px;
    border-top: 1px solid;
    margin-right: 13px;
}
.country-mapping-container .left-align{
    float: left;
}
.country-mapping-container .right-align{
    float: right;
}
.country-mapping-container .clear{
    clear: both;
}
.country-mapping-container .btn{
    height: 22px;
    border: none;
    border-radius: 0;
    background: #878787 !important;
    margin-left: -3px;
    padding-top: 2px;
    color: white;
    width: 120px;
}
.country-mapping-container .btn-add-user {
    height: 22px;
    border: none;
    border-radius: 0;
    background: #FFFFFF !important;
    margin-left: -3px;
    padding-top: 2px;
    width: 89px;
}
.country-mapping-container .btn-add-user:hover{
    background: #878787 !important;
    color: white;
}

.country-mapping-container .footer-container-div .fa{
    font-size: 14px;
    margin-right: 3px;
}
.sort:before{
    content: "";
    border-width: 0 4px 4px;
    border-style: solid;
    border-color: #000 transparent;
    visibility: visible;
    position: absolute;
    opacity: .3;
}
.sort:after{
    content: "";
    border-width: 0 4px 4px;
    border-style: solid;
    border-color: #000 transparent;
    visibility: visible;
    position: absolute;
    opacity: .3;
    margin-top: 2px;

}
.sort:before {
    margin-top: 8px;
    border-bottom: 0;
    border-left: 4px solid transparent;
    border-right: 4px solid transparent;
    border-top: 4px solid #000;
}

.country-mapping-container .table > thead > tr > th> i{padding-left: 4px;}
.country-mapping-container .sortable {  cursor: pointer;}
.country-mapping-container .st-sort-ascent i,.st-sort-descent i { padding-left: 4px; }
.country-mapping-container .st-sort-ascent i:before {  content: '\25B2';}
.country-mapping-container .st-sort-descent i:before {  content: '\25BC';}