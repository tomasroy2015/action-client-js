.d3-tip {
    line-height: 1;
    font-weight: bold;
    padding: 6px;
    background: #ffffff;
    border-radius: 4px;
    z-index: 100000 !important;
    -webkit-box-shadow: -1px 0px 10px 0px rgba(0,0,0,0.75);
    -moz-box-shadow: -1px 0px 10px 0px rgba(0,0,0,0.75);
    box-shadow: -1px 0px 10px 0px rgba(0,0,0,0.75);
}
.d3-tip:after {
    box-sizing: border-box;
    display: inline;
    font-size: 10px;
    width: 100%;
    line-height: 1;
    content: "\25BC";
    color: #ffffff;
    position: absolute;
    text-align: center;
}

/* Style northward tooltips differently */
.d3-tip.n:after {
    margin: -1px 0 0 0;
    top: 100%;
    left: 0;
}
