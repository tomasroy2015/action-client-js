.evaluationTemplateRoot {
    width            : 100%;
    height           : 100%;
    background-color : #fff;
    padding-bottom   : 0px;
    font-family: AkkuratStd-Regular;
}

.evaluationTemplateRoot .left-menu-evaluation-header {
    position         : relative;
    min-height       : 16px;
    padding-top      : 2px;
    padding-left     : 10px;
    /*height           : 75px;*/
    background-color : black;
    color            : white;
    width            : 100%;
    /*overflow         : hidden;*/
    display          : table;
}

.left-menu-evaluation-header .content-container {
    /*height: 30px;*/
    display: inline-block;
}
.evaluation-header-container{
    float: left;
}

.left-menu-evaluation-arrow-closer {
    position           : absolute;
    background         : #000000;
    right              : -16px;
    padding-top        : 3px;
    top                : 10px;
    text-align         : center;
    -webkit-box-shadow : 0 0 4px 2px rgba(85, 87, 89, 0.5);
    -moz-box-shadow    : 0 0 4px 2px rgba(85, 87, 89, 0.5);
    box-shadow         : 0 0 4px 2px rgba(85, 87, 89, 0.5);
    height             : 20px;
    width              : 16px;
}

.left-menu-evaluation-arrow-closer {
    cursor : pointer;
}

.left-menu-evaluation-arrow-closer .fa-caret-left:hover {
    color : #ddddde;
}
.left-menu-evaluation-header-info-container{
    display: table-cell;
    height: auto;
}
.left-menu-evaluation-top-panel-title {
    font-size     : 16px;
    font-weight   : bold;
    height        : 30px;
    margin-top    : 5px;
    /*overflow      : hidden;*/
    text-overflow : ellipsis;
    white-space   : nowrap;
}

.left-menu-evaluation-top-panel-title .scorebar-mainDiv {
    width       : 200px;
    text-align  : center;
    margin-left : 20px;
}

.left-menu-evaluation-top-panel-title .scorebar-scoreDiv {
    width       : 68px;
    max-width   : 55px;
    font-weight : bold;
    font-size   : 20px;
    text-align  : center;
}

.left-menu-evaluation-top-panel-title .customizable-scorebar-score-label span {
    display    : block;
    text-align : left;
    font-size  : small;
}

.evaluationTemplateRoot .row.button-bar {
    margin-top       : 5px;
    background-color : transparent;
    display          : block;
    width            : 200px;
    margin-left      : 0px;
}

.left-menu-evaluation-data-download {
    font-size : 11px;
    display   : inline-block;
    position  : absolute;
    right     : 10px;
    top       : 12px;
}

.left-menu-evaluation-data-download:hover {
    cursor : pointer;
}

div#myModalLabel {
    color         : #ffffff;
    font-size     : medium;
    font-weight   : bold;
    /*display       : inline-block;*/
    padding-right : 24px;
}

.evaluationTemplateRoot .button {
    color            : #ffffff;
    border           : none;
    font-size        : 11px;
    background-color : black;
    height           : 22px;
    border-radius    : 0px;
    padding-left     : 8px;
    padding-top      : 4px;
    text-align       : center;
}

.evaluationTemplateRoot .button:hover {
    background-color : #d3d3d3;
    color            : #000000;
}

.evaluationTemplateRoot .summary{
    width         : 75px;
    margin-left   :-10px;
}

.evaluationTemplateRoot .evaluation{
    width         : 85px;

}
.selected-evaluation-window-arrow {
    display     : block;
    position    : relative;
    margin-left : 100px;
    width       : 50px;
    margin-top  : -5px;
    height      : 16px;
}

.evaluation-window-container {
    position     : relative;
    padding-left : 0px;
    padding-top  : 0px;
}

.left-menu-evaluation-window-container {
    overflow : auto;
    height   : 100%;
    width    : 100%;
}

.left-menu-evaluation-window-container .left-menu-evaluation-window-content-container {
    width    : 100%;
    height   : 100%;
    position : absolute;
    top      : 0px;
}

.left-menu-summary-arrow-selected {
    margin-left : 18px;
}

.left-menu-evaluation-arrow-selected {
    margin-left : 100px;
}

.os-ratio-container {
    /*display       : inline-block;*/
    font-size     : 12px;
    font-weight   : bold;
    /*height        : 30px;*/
    overflow      : hidden;
    text-overflow : ellipsis;
    white-space   : nowrap;
    max-width     : 280px;
    /*width         : 100%;*/
}

.evaluation-window-container.show-by-animation {
    -webkit-animation-duration        : .4s;
    -moz-animation-duration           : .4s;
    -o-animation-duration             : .4s;
    animation-duration                : .4s;
    -webkit-animation-name            : show-left-menu-evaluation-details;
    -moz-animation-name               : show-left-menu-evaluation-details;
    -o-animation-name                 : show-left-menu-evaluation-details;
    animation-name                    : show-left-menu-evaluation-details;
    -webkit-animation-iteration-count : 1;
    -moz-animation-iteration-count    : 1;
    -o-animation-iteration-count      : 1;
    animation-iteration-count         : 1;
    -webkit-animation-timing-function : ease-in;
    -moz-animation-timing-function    : ease-in;
    -o-animation-timing-function      : ease-in;
    animation-timing-function         : ease-in;
    -webkit-animation-fill-mode       : forwards;
    -moz-animation-fill-mode          : forwards;
    -o-animation-fill-mode            : forwards;
    animation-fill-mode               : forwards;
    height                            : 88%;
}

@-webkit-keyframes show-left-menu-evaluation-details {
    0% {
        opacity : 0
    }
    25% {
        opacity : 0
    }
    50% {
        opacity : 0
    }
    75% {
        opacity : 0
    }
    100% {
        opacity : 1;
    }
}

@-moz-keyframes show-left-menu-evaluation-details {
    0% {
        opacity : 0
    }
    25% {
        opacity : 0
    }
    50% {
        opacity : 0
    }
    75% {
        opacity : 0
    }
    100% {
        opacity : 1;
    }
}

@-o-keyframes show-left-menu-evaluation-details {
    0% {
        opacity : 0
    }
    25% {
        opacity : 0
    }
    50% {
        opacity : 0
    }
    75% {
        opacity : 0
    }
    100% {
        opacity : 1;
    }
}

@keyframes show-left-menu-evaluation-details {
    0% {
        opacity : 0
    }
    25% {
        opacity : 0
    }
    50% {
        opacity : 0
    }
    75% {
        opacity : 0
    }
    100% {
        opacity : 1;
    }
}

