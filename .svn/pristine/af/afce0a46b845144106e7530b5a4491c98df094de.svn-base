.scorecard-details-wrapper {
  position: relative;
  width: 100%;
}
.scorecard-details-wrapper ul {
  padding: 0;
  margin: 0;
}

.scorecard-header-container {
  width: 100%;
  height: 62px;
  margin: 18px 0 0 18px;
}
.scorecard-header-container .scorecard-header-title {
  font-size: 1.4em;
  font-weight: bold;
}
.scorecard-header-container .scorecard-sub-header-title-container {
  display: inline-block;
}
.scorecard-header-container .scorecard-sub-header-title-container .os-ratio-container {
  float: right;
  font-size: 12px;
  font-weight: bold;
  height: 26px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 310px;
  margin-top: 16px;
  margin-left: 36px;
}
.scorecard-header-container .scorecard-sub-header-title-container .os-ratio-container .ratioContainer .ratioLabelContainer .ratio-label {
  color: #878787;
}
.scorecard-header-container .scorecard-sub-header-title-container .sub-header-title-wrapper {
  display: inline-block;
}
.scorecard-header-container .scorecard-sub-header-title-container .sub-header-title-wrapper .scorecard-sub-header-title {
  display: block;
  font-weight: bold;
}
.scorecard-header-container .scorecard-sub-header-title-container .sub-header-title-wrapper .scorecard-sub-header-evaluation {
  display: block;
  font-size: 0.9em;
  color: #878787;
}
.scorecard-header-container .scorecard-sub-header-title-container .scorecard-header-score-wrapper {
  display: inline-block;
  margin-left: 50px;
}

.scorecard-content-container:before {
  content: "";
  display: block;
  margin-bottom: 10px;
  margin-right: 20px;
  border-bottom: 1px solid #000000;
}

.scorecard-content-container {
  background: #fff;
  width: 100%;
  display: inline-block;
  margin-left: 20px;
}
.scorecard-content-container > ul.scorecard-container {
  display: inline-block;
}
.scorecard-content-container > ul.scorecard-container li.individual-scorecard {
  margin-right: 15px;
  display: inline-block;
  vertical-align: top;
}

.scorecard-header-container-ticket {
  height: 30px;
}

.scorecard-header-title-container {
  position: relative;
  display: block;
}
.scorecard-header-title-container .fsc-double-target-wrapper {
  position: absolute;
  display: inline-block;
  left: 668px;
  color: rgba(85, 87, 89, 0.4);
  font-size: 0.90em;
  width: 250px;
  text-align: right;
}
.scorecard-header-title-container .fsc-double-target-wrapper .fsc-target-legend {
  display: inline;
  position: relative;
}
.scorecard-header-title-container .fsc-double-target-wrapper .fsc-target-legend:before {
  content: '';
  position: absolute;
  margin-left: -10px;
  width: 3px;
  height: 10px;
  background: #555759;
  top: 0;
}
.scorecard-header-title-container .fsc-double-target-wrapper .fsc-target-legend:after {
  content: '/';
  position: absolute;
  padding-left: 10px;
}
.scorecard-header-title-container .fsc-double-target-wrapper .fsc-double-target-legend {
  display: inline;
  position: relative;
  padding-left: 30px;
}
.scorecard-header-title-container .fsc-double-target-wrapper .fsc-double-target-legend:before {
  content: '';
  position: absolute;
  margin-left: -10px;
  width: 3px;
  height: 10px;
  background: rgba(85, 87, 89, 0.5);
  top: 0;
}

