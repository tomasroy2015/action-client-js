    }
    50% {
        width  : 750px;
        height : 650px;
    }
    75% {
        width  : 600px;
        height : 550px;
    }
    100% {
        width  : 280px;
        height : 520px;
    }
}

@-o-keyframes collapse-details {
    0% {
        width  : calc(100% - 300px);
        height : calc(100% - 115px);
    }
    25% {
        width  : 850px;
