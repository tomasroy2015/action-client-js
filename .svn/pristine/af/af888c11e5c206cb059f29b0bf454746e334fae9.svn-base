.summary-popup {
    position                   : fixed;
    z-index                    : 1;
    display                    : none;
    overflow                   : hidden;
    -webkit-overflow-scrolling : touch;
    outline                    : 0;
    background-color           : #ffffff;
    border                     : 1px solid #d3d3d3;
    box-shadow                 : 1px 1px 1px #d3d3d3;
    -webkit-box-shadow         : 1px 1px 1px #d3d3d3;
    -moz-box-shadow            : 1px 1px 1px #d3d3d3;;
}

.show-popup {
    width                      : 274px;
