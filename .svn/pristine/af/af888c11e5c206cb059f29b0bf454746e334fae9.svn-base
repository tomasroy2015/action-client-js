.expand-details {
    -webkit-animation-duration        : .3s;
    -moz-animation-duration           : .3s;
    -o-animation-duration             : .3s;
    animation-duration                : 0.3s;
    -webkit-animation-name            : expand-details;
    -moz-animation-name               : expand-details;
    -o-animation-name                 : expand-details;
    animation-name                    : expand-details;
    -webkit-animation-iteration-count : 1;
    -moz-animation-iteration-count    : 1;
    -o-animation-iteration-count      : 1;
    animation-iteration-count         : 1;
    -webkit-animation-timing-function : ease-out;
    -moz-animation-timing-function    : ease-out;
    -o-animation-timing-function      : ease-out;
    animation-timing-function         : ease-out;
    -webkit-animation-fill-mode       : forwards;
    -moz-animation-fill-mode          : forwards;
    -o-animation-fill-mode            : forwards;
    animation-fill-mode               : forwards;
}

@-webkit-keyframes expand-details {
    0% {
        width  : 30%;
        height : 30%;
    }
    25% {
        width  : 40%;
        height : 50%;
    }
    50% {
        width  : 50%;
        height : 70%;
    }
    75% {
        width  : 60%;
        height : 85%;
    }
    100% {
        width  : calc(100% - 287px); /* 300px */
        height : calc(100% - 115px);
    }
}

@-moz-keyframes expand-details {
    0% {
        width  : 30%;
        height : 30%;
    }
    25% {
        width  : 40%;
        height : 50%;
    }
    50% {
        width  : 50%;
        height : 70%;
    }
    75% {
        width  : 60%;
        height : 85%;
    }
    100% {
        width  : calc(100% - 287px);
        height : calc(100% - 115px);
    }
}

@-o-keyframes expand-details {
    0% {
        width  : 30%;
        height : 30%;
    }
    25% {
        width  : 40%;
        height : 50%;
    }
    50% {
        width  : 50%;
        height : 70%;
    }
    75% {
        width  : 60%;
        height : 85%;
    }
    100% {
        width  : calc(100% - 287px);
        height : calc(100% - 115px);
    }
}

@keyframes expand-details {
    0% {
        width  : 30%;
        height : 30%;
    }
    25% {
        width  : 40%;
        height : 50%;
    }
    50% {
        width  : 50%;
        height : 70%;
    }
    75% {
        width  : 60%;
        height : 85%;
    }
    100% {
          width  : calc(100% - 287px);
        height : calc(100% - 115px);
        /*left:10px;*/
        /*width:95%;*/
    }
}

