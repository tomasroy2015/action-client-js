.collapse-details {
    -webkit-animation-duration        : .3s;
    -moz-animation-duration           : .3s;
    -o-animation-duration             : .3s;
    animation-duration                : .3s;
    -webkit-animation-name            : collapse-details;
    -moz-animation-name               : collapse-details;
    -o-animation-name                 : collapse-details;
    animation-name                    : collapse-details;
    -webkit-animation-iteration-count : 1;
    -moz-animation-iteration-count    : 1;
    -o-animation-iteration-count      : 1;
    animation-iteration-count         : 1;
    -webkit-animation-timing-function : ease-in;
    -moz-animation-timing-function    : ease-in;
    -o-animation-timing-function      : ease-in;
    animation-timing-function         : ease-in;
    -webkit-animation-fill-mode       : backwards;
    -moz-animation-fill-mode          : backwards;
    -o-animation-fill-mode            : backwards;
    animation-fill-mode               : backwards;
    /*position:absolute;*/
    /*min-height: 350px;*/
}

@-webkit-keyframes collapse-details {
    0% {
        width  : calc(100% - 300px);
        height : calc(100% - 115px);
    }
    25% {
        width  : 850px;
        height : 750px;
    }
    50% {
        width  : 750px;
        height : 650px;
    }
    75% {
        width  : 600px;
        height : 550px;
    }
    100% {
        width  : 280px;
        height : 520px;
    }
}

@-moz-keyframes collapse-details {
    0% {
        width  : calc(100% - 300px);
        height : calc(100% - 115px);
    }
    25% {
