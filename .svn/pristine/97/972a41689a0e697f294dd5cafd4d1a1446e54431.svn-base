/* All external import files
======================================================*/
@import '_Colors';
@import '_Mixins';

$score-item-height:40px;
$calculate-right:11px;

ul.evaluation-summary-question {
  display: inline-block;
  width: 255px;
  position: relative;
  height: 196px;
  overflow-y: auto;
  overflow-x: hidden;
  border-top: 1px solid #d3d3d3;
  font-family: "AkkuratStd-Regular";
  padding: 0px;
//  border-bottom: 1px solid #d3d3d3;
  font-family: "AkkuratStd-Regular";
  font:"akkuratstd-regular";
  vertical-align: bottom;

  li.evaluation-summary-questionListItem {
    position: relative;
//    padding: px;
//    font-size: 8px;
    text-align: right;
    border-bottom: 1px solid #d3d3d3;
    vertical-align: bottom;
    display: inline-block;

    .evaluation-summary-questionListItemDiv{
      position: relative;
      text-align: left;

      .evaluation-summary-questionContainerDiv {
//        font-size: 10px;

        ul.score-frequency-list-container{
          position: relative;
          width: 244px;
          display: inline-block;
          height: 38px;
          max-height: 38px;
          overflow-y: hidden;
          margin: 0;
          padding-left: 8px;
          list-style: none;
          bottom: 0px;
          text-overflow: ellipsis;
          vertical-align: bottom;

          .score-frequency-header{
            display: flex;
            /* padding: 5px; */
            text-align: center;
            margin: 5px 0px 0px -10px;
            border-bottom: 1px solid #d3d3d3;

            .frequency-label{
              text-align: left;
              font-size: 11px;
              width: 70px;
              margin-right: 18px;
              margin-top: 4px;
            }

            .negative-label{
              margin-right: 2px;
              margin-top: 12px;
              /* padding-top: 5px; */
              width: 18px;
              height: 18px;
              font-size: 14px;
              font-weight: bold;
              color: #ffffff;
              background: #ff0000;
            }
            .positive-label{
              margin-right: 2px;
              margin-top: 12px;
              /* padding-top: 5px; */
              width: 18px;
              height: 18px;
              font-size: 14px;
              font-weight: bold;
              color: #ffffff;
              background: #65B32E;
            }

            .header-percentage{
              margin-top: 12px;
              width: 18px;
              height: 18px;
              font-size: 14px;
              font-weight: bold;
              color: #000000;
            }
          }

          li.score-frequency-listItem
          {
            position: relative;
            vertical-align: bottom;
            display: inline-block;
            text-align: center;
            margin-bottom: -20px;

            .question-index{
              font-size: 16px;
              font-weight: bold;
              text-align: right;
              margin-left: -8px;
              width: 12px;
              display: block;
            }

            .negative-frequency-bar{
//              background-color : $GIARTE_RED;
              display: block;
              width: 15px;
              max-height: 35px;
              margin-right: 2px;
              margin-bottom: 1px;
            }:hover{cursor: pointer;}

            .positive-frequency-bar{
//              background-color : $GIARTE_GREEN;
              display: block;
              width: 15px;
              max-height: 35px;
              margin-right: 2px;
              margin-bottom: 1px;
            }:hover{cursor: pointer;}

            .evaluation-summary-questionText {
              color : $GIARTE_BLACK;
            }

            .evaluation-summary-questionLabelDiv {
              text-align: left;
              /* word-wrap: break-word; */
              width: 119px;
              margin-left: 3px;
              margin-bottom: 2px;
              font-size: 9.9px;
              overflow: hidden;
              text-overflow: ellipsis;
              display: -webkit-box;
              line-height: 18px;
              max-height: 36px;
              -webkit-line-clamp: 2;
              -ms-line-clamp: 2;
              -o-line-clamp: 2;
              line-clamp: 2;
              -webkit-box-orient: vertical;
              -ms-box-orient: vertical;
              -o-box-orient: vertical;
              box-orient: vertical;
            }
            .freq-ques-score{
              margin-left: 2px;
              font-size: 11px;
              font-weight: bold;
              display: block;
            }
          }
        }

        ul.ses-score-frequency-list-container{
          position: relative;
          width: 244px;
          display: inline-block;
          height: 88px;
          margin: 0;
          padding-left: 8px;
          list-style: none;
          bottom: 0px;

          .score-frequency-header {
            display: flex;
            /* padding: 5px; */
            text-align: center;
            margin: 5px 0px 0px -10px;
            border-bottom: 1px solid #d3d3d3;

            .frequency-label {
              text-align: left;
              font-size: 11px;
              width: 77px;
              margin-right: 18px;
              margin-top: 4px;
            }

            .negative-label {
              margin-right: 2px;
              margin-top: 12px;
              /* padding-top: 5px; */
              width: 18px;
              height: 18px;
              font-size: 14px;
              font-weight: bold;
              color: #ffffff;
              background: #ff0000;
            }
            .positive-label {
              margin-right: 2px;
              margin-top: 12px;
              /* padding-top: 5px; */
              width: 18px;
              height: 18px;
              font-size: 14px;
              font-weight: bold;
              color: #ffffff;
              background: #65B32E;
            }

            .header-percentage {
              margin-top: 12px;
              width: 18px;
              height: 18px;
              font-size: 14px;
              font-weight: bold;
              color: #000000;
            }
          }

          li.ses-score-frequency-listItem{
            position: relative;
            vertical-align: bottom;
            display: inline-block;
            text-align: center;
            margin-bottom: -20px;
            font-size: 11px;
            font-weight: bold;

            .ses-question-index{
              font-size: 16px;
              font-weight: bold;
              text-align: left;
              margin-left: -8px;
            }

            .ses-negative-frequency-bar{
              //              background-color : $GIARTE_RED;
              display          : block;
              width: 16px;
              max-height: 35px;
              margin-right: 2px;
            }:hover{cursor: pointer;}

            .ses-positive-frequency-bar{
              //              background-color : $GIARTE_GREEN;
              display          : block;
              width: 16px;
              max-height: 35px;
              margin-right: 2px;
            }:hover{cursor: pointer;}

            .ses-evaluation-summary-questionText {
              color : $GIARTE_BLACK;
            }

            .ses-evaluation-summary-questionLabelDiv {
              text-align: left;
              word-wrap: break-word;
              width: 83px;
              padding-left: 6px;
              font-size: 9.9px;
            }
            .ses-freq-ques-score{
              margin-left: 3px;
              font-size: 11px;
              font-weight: bold;
            }
          }
        }
      }
    }
  }

}


.evaluation-summary-questionScoreDiv1 {
  width: 30px;
  display: inline-block;
  float: right;
  font-weight: bold;
  padding-top: 5px;
}

.evaluation-canvasContainer {
  height : 28px;
}

.evaluation-summary-questionScorebar {
  background-color : #ff0000;
  padding-left     : 0;
  padding-right    : 0;
  margin-left      : 2px;
  margin-right     : auto;
  display          : block;
  float            : left;
  top              : 5px
}


.score-frequency-container{
  display: flex;
  padding: 5px;
  text-align: center;
}
.score-frequency-container  .negative-label {
  margin-right: 2px;
  /* padding-top: 5px; */
  width: 20px;
  height: 20px;
  font-size: 16px;
  font-weight: bold;
  color: #ffffff;
  background: #ff0000;
}
.score-frequency-container  .positive-label {
  margin-right: 2px;
  /* padding-top: 5px; */
  width: 20px;
  height: 20px;
  font-size: 16px;
  font-weight: bold;
  color: #ffffff;
  background: #008000;
}